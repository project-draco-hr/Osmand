{
  if (rst != ResultSet.TYPE_FORWARD_ONLY)   throw new SQLException("SQLite only supports TYPE_FORWARD_ONLY cursors");
  if (rsc != ResultSet.CONCUR_READ_ONLY)   throw new SQLException("SQLite only supports CONCUR_READ_ONLY cursors");
  if (rsh != ResultSet.CLOSE_CURSORS_AT_COMMIT)   throw new SQLException("SQLite only supports closing cursors at commit");
}
