{
  Drawable d=RenderingIcons.getBigIcon(ctx,fileName);
  if (d != null) {
    boolean light=app.getSettings().isLightContent();
    d.setColorFilter(app.getResources().getColor(light ? R.color.icon_color : R.color.icon_color_light),PorterDuff.Mode.SRC_IN);
    return d;
  }
 else {
    return null;
  }
}
