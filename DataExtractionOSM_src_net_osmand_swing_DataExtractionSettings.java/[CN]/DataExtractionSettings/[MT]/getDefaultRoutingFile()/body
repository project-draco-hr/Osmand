{
  String routingFile=preferences.get("routing_file",null);
  if (routingFile == null) {
    return null;
  }
  String[] files=routingFile.split(",");
  List<File> fs=new ArrayList<File>();
  for (  String f : files) {
    if (new File(f.trim()).exists()) {
      fs.add(new File(f.trim()));
    }
  }
  return fs.toArray(new File[fs.size()]);
}
