{
  if (f.getName().endsWith(IndexConstants.TRANSPORT_INDEX_EXT)) {
    TransportIndexRepositoryOdb repository=new TransportIndexRepositoryOdb();
    progress.startTask(Messages.getMessage("indexing_transport") + f.getName(),-1);
    try {
      boolean initialized=repository.initialize(progress,f);
      if (initialized) {
        transportRepositories.add(repository);
      }
 else {
        warnings.add(MessageFormat.format(Messages.getMessage("version_index_is_not_supported"),f.getName()));
      }
    }
 catch (    SQLiteException e) {
      log.error("Exception reading " + f.getAbsolutePath(),e);
      warnings.add(MessageFormat.format(Messages.getMessage("version_index_is_not_supported"),f.getName()));
    }
  }
}
