{
  log.info("On low memory : cleaning tiles - size = " + cacheOfImages.size());
  clearTiles();
  for (  AmenityIndexRepository r : amenityRepositories) {
    r.clearCache();
  }
  for (  RegionAddressRepository r : addressMap.values()) {
    r.clearCache();
  }
  renderer.clearCache();
  System.gc();
}
