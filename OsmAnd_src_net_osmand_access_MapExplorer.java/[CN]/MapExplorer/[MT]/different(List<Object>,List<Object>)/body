{
  if ((l1 != null) && !l1.isEmpty()) {
    if ((l2 != null) && !l2.isEmpty()) {
      if (l1.size() != l2.size())       return true;
      for (int i=0; i < l1.size(); i++)       if (l1.get(i) != l2.get(i))       return true;
      return false;
    }
    return true;
  }
  return ((l2 != null) && !l2.isEmpty());
}
