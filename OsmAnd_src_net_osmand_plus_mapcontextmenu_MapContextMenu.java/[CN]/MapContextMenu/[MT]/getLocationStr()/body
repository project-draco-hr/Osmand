{
  if (menuController != null && menuController.needTypeStr()) {
    return typeStr;
  }
 else {
    if (Algorithms.isEmpty(streetStr)) {
      return PointDescription.getLocationName(mapActivity,latLon.getLatitude(),latLon.getLongitude(),true).replaceAll("\n","");
    }
 else {
      return streetStr;
    }
  }
}
