{
  leftIconId=0;
  favActionIconId=0;
  leftIcon=null;
  secondLineIcon=null;
  if (menuController != null) {
    leftIconId=menuController.getLeftIconId();
    leftIcon=menuController.getLeftIcon();
    secondLineIcon=menuController.getSecondLineIcon();
    favActionIconId=menuController.getFavActionIconId();
  }
}
