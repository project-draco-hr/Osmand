{
  builder.setLength(0);
  builder.append(map.getName());
  if (map instanceof SQLiteTileSource) {
    builder.append('@');
  }
 else {
    builder.append('/');
  }
  builder.append(zoom).append('/').append(x).append('/').append(y).append(map.getTileFormat()).append(".tile");
  String file=builder.toString();
  return file;
}
