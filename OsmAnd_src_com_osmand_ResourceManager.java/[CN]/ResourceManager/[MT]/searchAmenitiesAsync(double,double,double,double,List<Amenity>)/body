{
  for (  AmenityIndexRepository index : amenityRepositories) {
    if (index.checkContains(topLatitude,leftLongitude,bottomLatitude,rightLongitude)) {
      if (!index.checkCachedAmenities(topLatitude,leftLongitude,bottomLatitude,rightLongitude,toFill,true)) {
        asyncLoadingTiles.requestToLoadAmenities(new AmenityLoadRequest(index,topLatitude,leftLongitude,bottomLatitude,rightLongitude));
      }
    }
  }
}
