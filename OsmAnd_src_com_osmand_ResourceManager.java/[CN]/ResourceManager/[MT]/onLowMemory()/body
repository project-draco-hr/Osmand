{
  log.info("On low memory : cleaning tiles - size = " + cacheOfImages.size());
  ArrayList<String> list=new ArrayList<String>(cacheOfImages.keySet());
  for (int i=0; i < list.size() / 2; i++) {
    Bitmap bmp=cacheOfImages.remove(list.get(i));
    if (bmp != null) {
      bmp.recycle();
    }
  }
  System.gc();
}
