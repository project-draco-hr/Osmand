{
  List<SearchObject> res=new ArrayList<>();
  for (  SearchObject item : searchObjects) {
    if (cancelled) {
      break;
    }
    if (keyword.isEmpty() || item.getName(searchScope.getLang()).toLowerCase().contains(keyword) || item.getNativeName().toLowerCase().contains(keyword)) {
      res.add(item);
    }
  }
  return res;
}
