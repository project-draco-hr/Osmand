{
  typeUse.clear();
  int mask=(1 << 16) - 1;
  int k=(int)(i & mask);
  int r=0;
  if (k > 0) {
    r=k;
    i>>=16;
    k=(int)(i & mask);
    if (k > 0) {
      typeUse.add(k);
      i>>=16;
      k=(int)(i & mask);
      if (k > 0) {
        typeUse.add(k);
        i>>=16;
        k=(int)(i & mask);
        if (k > 0) {
          typeUse.add(k);
          i>>=16;
        }
      }
    }
  }
  return r;
}
