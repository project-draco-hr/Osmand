{
  long i=0;
  int ind=0;
  int sh=0;
  if (typeUse.size() > 3) {
    log.error("Types for low index way more than 4");
  }
  i|=(mainType << sh);
  if (typeUse.size() > ind) {
    sh+=16;
    i|=((long)typeUse.get(ind++) << sh);
    if (typeUse.size() > ind) {
      sh+=16;
      i|=((long)typeUse.get(ind++) << sh);
      if (typeUse.size() > ind) {
        sh+=16;
        i|=((long)typeUse.get(ind++) << sh);
      }
    }
  }
  return i;
}
