{
  String id=constructRuleKey(tag,val);
  if (!types.containsKey(id)) {
    MapRouteType rt=new MapRouteType();
    rt.id=types.size() + 1;
    rt.tag=tag;
    rt.value=val;
    types.put(id,rt);
    listTypes.add(rt);
    if (tag.equals("ref")) {
      refRuleType=rt;
    }
    if (tag.equals("name")) {
      nameRuleType=rt;
    }
  }
  MapRouteType type=types.get(id);
  type.freq++;
  return type;
}
