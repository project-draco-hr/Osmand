{
  ts.clear();
  if (bs != null && bs.length > 0) {
    for (int j=0; j < bs.length; j+=4) {
      ts.add(Algoritms.parseIntFromBytes(bs,j));
    }
  }
  if (bt != null && bt.length > 0) {
    for (int j=0; j < bt.length; j+=4) {
      ts.add(Algoritms.parseIntFromBytes(bt,j));
    }
  }
  ts.sort();
}
