{
  if (mapPath.canRead()) {
    for (    File gpxFile : listFilesSorted(mapPath)) {
      if (gpxFile.isFile() && gpxFile.getName().endsWith(".gpx")) {
        LocalIndexInfo info=new LocalIndexInfo(LocalIndexType.GPX_DATA,gpxFile,backup);
        result.add(info);
        loadTask.loadFile(info);
      }
    }
  }
}
