{
  if ((map == null && zoom < 23) || (map != null && (map.getMaximumZoomSupported() + OVERZOOM_IN) >= zoom && map.getMinimumZoomSupported() <= zoom)) {
    this.zoom=zoom;
    refreshMap();
    if (notify && locationListener != null) {
      locationListener.locationChanged(latitude,longitude,this);
    }
  }
}
