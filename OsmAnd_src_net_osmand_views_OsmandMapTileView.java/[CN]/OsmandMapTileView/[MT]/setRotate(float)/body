{
  float diff=this.rotate - rotate;
  if (diff > 4 || diff < -4) {
    this.rotate=rotate;
    float rotateRad=(float)Math.toRadians(rotate);
    this.rotateCos=FloatMath.cos(rotateRad);
    this.rotateSin=FloatMath.sin(rotateRad);
    animatedDraggingThread.stopAnimating();
    refreshMap();
  }
}
