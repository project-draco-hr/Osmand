{
  double dmax=-1;
  int index=-1;
  for (int i=start + 1; i <= end - 1; i++) {
    double d=orthogonalDistance(gw,start,end,gw.px.get(i),gw.py.get(i),false);
    if (d > dmax) {
      dmax=d;
      index=i;
    }
  }
  if (dmax >= epsilon) {
    simplifyDouglasPeucker(gw,epsilon,ints,start,index);
    simplifyDouglasPeucker(gw,epsilon,ints,index,end);
  }
 else {
    ints.add(end);
  }
}
