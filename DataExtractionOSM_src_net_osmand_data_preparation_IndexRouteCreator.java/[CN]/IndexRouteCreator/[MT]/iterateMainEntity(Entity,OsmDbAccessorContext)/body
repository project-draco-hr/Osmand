{
  if (es instanceof Way) {
    Way e=(Way)es;
    boolean encoded=routeTypes.encodeEntity(e,outTypes,names);
    if (encoded) {
      ctx.loadEntityWay(e);
      routeTypes.encodePointTypes(e,pointTypes);
      addWayToIndex(e.getId(),e.getNodes(),mapRouteInsertStat,routeTree);
      generalizeWay(e);
    }
  }
}
