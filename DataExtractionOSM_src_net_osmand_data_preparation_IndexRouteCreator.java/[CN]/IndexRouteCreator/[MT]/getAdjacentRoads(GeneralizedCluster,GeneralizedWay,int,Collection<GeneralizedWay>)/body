{
  gcluster=getCluster(gw,i,gcluster);
  Object o=gcluster.map.get(gw.getLocation(i));
  if (o instanceof LinkedList) {
    Iterator it=((LinkedList)o).iterator();
    while (it.hasNext()) {
      GeneralizedWay next=(GeneralizedWay)it.next();
      if (next.id != gw.id) {
        collection.add(next);
      }
    }
  }
}
