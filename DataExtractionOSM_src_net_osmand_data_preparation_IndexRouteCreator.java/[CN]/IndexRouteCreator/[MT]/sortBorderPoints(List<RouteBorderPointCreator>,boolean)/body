{
  Collections.sort(pnts,new Comparator<RouteBorderPointCreator>(){
    @Override public int compare(    RouteBorderPointCreator o1,    RouteBorderPointCreator o2){
      int t1=x ? o1.x : o1.y;
      int t2=x ? o2.x : o2.y;
      if (t1 == t2) {
        return 0;
      }
      return t1 < t2 ? -1 : 1;
    }
  }
);
}
