{
switch (keyCode) {
case KeyEvent.KEYCODE_MENU:
    onKeyUpPanel(Window.FEATURE_OPTIONS_PANEL,event);
  return true;
case KeyEvent.KEYCODE_BACK:
PanelFeatureState st=getPanelState(Window.FEATURE_OPTIONS_PANEL,false);
if (st != null && st.isOpen) {
closePanel(st,true);
return true;
}
if (onBackPressed()) {
return true;
}
break;
}
return false;
}
