{
  final boolean isPre21=Build.VERSION.SDK_INT < 21;
  if (mTintViewInflater == null) {
    mTintViewInflater=new TintViewInflater(mContext);
  }
  final boolean inheritContext=isPre21 && mSubDecorInstalled && parent != null && parent.getId() != android.R.id.content;
  return mTintViewInflater.createView(parent,name,context,attrs,inheritContext,isPre21);
}
