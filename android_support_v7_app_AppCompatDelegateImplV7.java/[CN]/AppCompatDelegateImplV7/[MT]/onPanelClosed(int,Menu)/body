{
  if (featureId == FEATURE_ACTION_BAR) {
    ActionBar ab=getSupportActionBar();
    if (ab != null) {
      ab.dispatchMenuVisibilityChanged(false);
    }
    return true;
  }
 else   if (featureId == FEATURE_OPTIONS_PANEL) {
    PanelFeatureState st=getPanelState(featureId,true);
    if (st.isOpen) {
      closePanel(st,false);
    }
  }
  return false;
}
