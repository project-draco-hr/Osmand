{
  SQLiteAPI.SQLiteConnection db=openConnection(false);
  if (db != null) {
    try {
      db.execSQL("UPDATE " + HISTORY_TABLE_NAME + " SET "+ HISTORY_COL_COUNT+ " = ? WHERE "+ HISTORY_COL_NAME+ " = ?",new Object[]{e.getCount(),e.getName()});
    }
  finally {
      db.close();
    }
    return true;
  }
  return false;
}
