{
  List<HistoryEntry> entries=new ArrayList<HistoryEntry>();
  SQLiteAPI.SQLiteConnection db=openConnection(true);
  if (db != null) {
    try {
      SQLiteAPI.SQLiteCursor query=db.rawQuery("SELECT " + HISTORY_COL_NAME + " FROM "+ HISTORY_TABLE_NAME+ " ORDER BY "+ HISTORY_COL_COUNT+ " DESC",null);
      if (query.moveToFirst()) {
        do {
          HistoryEntry e=new HistoryEntry(query.getString(0),query.getInt(1));
          entries.add(e);
        }
 while (query.moveToNext());
      }
      query.close();
    }
  finally {
      db.close();
    }
  }
  return entries;
}
