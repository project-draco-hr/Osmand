{
  addressSearchStreetStat.setLong(1,city.getId());
  addressSearchStreetStat.setString(2,cityPart);
  addressSearchStreetStat.setString(3,name);
  ResultSet rs=addressSearchStreetStat.executeQuery();
  Long foundId=null;
  if (rs.next()) {
    foundId=rs.getLong(1);
  }
  rs.close();
  return foundId;
}
