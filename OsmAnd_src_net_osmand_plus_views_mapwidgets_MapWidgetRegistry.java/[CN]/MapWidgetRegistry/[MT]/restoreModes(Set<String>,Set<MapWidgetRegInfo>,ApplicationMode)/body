{
  for (  MapWidgetRegInfo m : mi) {
    if (m.preference == null) {
      if (m.visibleModes.contains(mode)) {
        set.add(m.key);
      }
 else       if (m.visibleCollapsible != null && m.visibleCollapsible.contains(mode)) {
        set.add("+" + m.key);
      }
 else {
        set.add("-" + m.key);
      }
    }
  }
}
