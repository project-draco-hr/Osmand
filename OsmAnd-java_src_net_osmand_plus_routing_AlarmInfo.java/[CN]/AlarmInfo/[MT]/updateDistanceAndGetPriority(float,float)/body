{
  this.distance=distance;
  this.time=time;
  if (distance > 1500) {
    return 0;
  }
  if (time < 8 || distance < 100 || type == SPEED_LIMIT) {
    return type;
  }
  if (type == SPEED_CAMERA && (time < 15 || distance < 150)) {
    return type;
  }
  if (time < 10 || distance < 150) {
    return type + MAXIMUM;
  }
  return 0;
}
