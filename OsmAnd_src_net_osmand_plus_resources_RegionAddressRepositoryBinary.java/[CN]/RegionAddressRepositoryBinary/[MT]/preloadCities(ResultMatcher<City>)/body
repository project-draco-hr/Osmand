{
  if (cities.isEmpty()) {
    try {
      List<City> cs=file.getCities(region,BinaryMapIndexReader.buildAddressRequest(resultMatcher),BinaryMapAddressReaderAdapter.CITY_TOWN_TYPE);
      for (      City c : cs) {
        cities.put(c.getId(),c);
      }
    }
 catch (    IOException e) {
      log.error("Disk operation failed",e);
    }
  }
}
