{
  if (nativeRenderer.getQueue().isEmpty()) {
    try {
      nativeRenderingImg=nativeLibRendering.renderImage(sleft - EXPAND_X * cf,sright + EXPAND_X * cf,stop - EXPAND_Y * cf,sbottom + EXPAND_Y * cf,z);
      nativeLatLon=latLon;
      nativeZoom=z;
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    repaint();
  }
}
