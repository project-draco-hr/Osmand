{
  ResultSet rs=stat.executeQuery("SELECT MIN(minLon), MAX(maxLat), MAX(maxLon), MIN(minLat) FROM " + IndexConstants.indexMapLocationsTable);
  RectF bounds=new RectF();
  if (rs.next()) {
    bounds.set(rs.getFloat(1),rs.getFloat(2),rs.getFloat(3),rs.getFloat(4));
  }
  rs.close();
  return bounds;
}
