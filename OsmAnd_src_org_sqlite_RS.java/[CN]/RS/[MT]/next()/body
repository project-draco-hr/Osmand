{
  if (!open)   return false;
  lastCol=-1;
  if (row == 1) {
    row++;
    return true;
  }
  if (maxRows != 0 && row > maxRows)   return false;
  if (limitRows != 0 && row >= limitRows)   return false;
switch (db.step(stmt.pointer)) {
case SQLITE_DONE:
    close();
  return false;
case SQLITE_ROW:
row++;
return true;
case SQLITE_BUSY:
throw new SQLException("database locked");
default :
db.throwex();
return false;
}
}
