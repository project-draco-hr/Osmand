{
  String s=preferences.get("suffixes_normalize_streets",null);
  if (s == null) {
    return SUFFIXES;
  }
  List<String> l=new ArrayList<String>();
  int i=0;
  int nextI=0;
  while ((nextI=s.indexOf(',',i)) >= 0) {
    String t=s.substring(i,nextI).trim();
    if (t.length() > 0) {
      l.add(t);
    }
    i=nextI + 1;
  }
  return l.toArray(new String[l.size()]);
}
