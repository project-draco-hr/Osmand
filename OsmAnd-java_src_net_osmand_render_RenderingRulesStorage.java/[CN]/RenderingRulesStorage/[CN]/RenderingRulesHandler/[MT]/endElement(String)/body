{
  if (isCase(name)) {
    stack.pop();
  }
 else   if (isSwitch(name)) {
    GroupRules group=(GroupRules)stack.pop();
    if (stack.size() == 0) {
      group.registerGlobalRules(state);
    }
  }
 else   if (isApply(name)) {
    stack.pop();
  }
 else   if ("renderingAttribute".equals(name)) {
    stack.pop();
  }
}
