{
  files.remove(file);
  if (nativeFiles.contains(file)) {
    NativeOsmandLibrary lib=NativeOsmandLibrary.getLoadedLibrary();
    if (lib != null) {
      lib.closeMapFile(file);
      nativeFiles.remove(file);
    }
  }
  try {
    c.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
