{
  if (prevY > y) {
    int tx=prevX;
    int ty=prevY;
    x=prevX;
    y=prevY;
    prevX=tx;
    prevY=ty;
  }
  if (y == middleY || prevY == middleY) {
    middleY-=1;
  }
  if (prevY > middleY || y < middleY) {
    return Integer.MIN_VALUE;
  }
 else {
    if (y == prevY) {
      return x;
    }
    double rx=x + ((double)middleY - y) * ((double)x - prevX) / (((double)y - prevY));
    return (int)rx;
  }
}
