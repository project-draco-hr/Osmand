{
  String text=source.substring(offset,offset + length);
  int start=text.indexOf("/*-[");
  int end=text.lastIndexOf("]-*/");
  if (start == -1 || end <= start) {
    start=text.indexOf("/*-{");
    end=text.lastIndexOf("}-*/");
  }
  if (start == -1 || end <= start) {
    return null;
  }
  return text.substring(start + 4,end);
}
