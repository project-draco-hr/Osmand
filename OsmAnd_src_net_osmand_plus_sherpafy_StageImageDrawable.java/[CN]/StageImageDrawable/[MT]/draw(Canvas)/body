{
  Rect bs=getBounds();
  int min=(int)(Math.min(bs.width(),bs.height()) - 8 * density);
  int c=bs.width() / 2;
  int r=(int)(min / 2);
  canvas.drawCircle(c,c,r,paintInnerCircle);
  if (drawable != null) {
    drawable.draw(canvas);
  }
 else   if (text != null) {
    canvas.drawText(text,c,r * 3 / 4,textPaint);
  }
}
