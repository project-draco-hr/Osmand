{
  boolean isPushed=false;
  for (  WeakReference<Fragment> ref : fragSet) {
    Fragment f=ref.get();
    if (f instanceof NewLocalIndexesFragment) {
      if (f.isAdded()) {
        isPushed=true;
        ((NewLocalIndexesFragment)f).onCategorizationFinished(filtered,cats);
      }
    }
  }
  if (!isPushed) {
    this.cats=cats;
  }
}
