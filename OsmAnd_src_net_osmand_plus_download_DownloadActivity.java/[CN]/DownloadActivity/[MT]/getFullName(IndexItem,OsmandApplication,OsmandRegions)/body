{
  String eName;
  List<IndexItem> forCat=new ArrayList<IndexItem>();
  forCat.add(e);
  List<IndexItemCategory> category=IndexItemCategory.categorizeIndexItems(app,forCat);
  if (category.size() != 0) {
    eName=e.getVisibleDescription(app) + "\n" + category.get(0).name+ " "+ e.getVisibleName(app,osmandRegions);
  }
 else {
    eName=e.getVisibleDescription(app) + "\n" + e.getVisibleName(app,osmandRegions);
  }
  return eName;
}
