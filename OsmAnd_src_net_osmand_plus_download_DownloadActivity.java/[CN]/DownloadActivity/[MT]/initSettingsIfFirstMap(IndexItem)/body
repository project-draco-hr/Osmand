{
  OsmandSettings.CommonPreference<Boolean> isFirstMapDownloadedPreference=getMyApplication().getSettings().registerBooleanPreference(FIRST_MAP_DOWNLOADED,false).makeGlobal();
  boolean wasFirstMapDownloaded=isFirstMapDownloadedPreference.get();
  if (!wasFirstMapDownloaded) {
    isFirstMapDownloadedPreference.set(true);
  }
}
