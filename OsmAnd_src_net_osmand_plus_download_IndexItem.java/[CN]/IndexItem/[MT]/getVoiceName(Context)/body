{
  try {
    String nm=getBasename().replace('-','_').replace(' ','_');
    if (nm.endsWith("-tts")) {
      nm=nm.substring(0,nm.length() - 4);
    }
    Field f=R.string.class.getField("lang_" + nm);
    if (f != null) {
      Integer in=(Integer)f.get(null);
      return ctx.getString(in);
    }
  }
 catch (  Exception e) {
    System.err.println(e.getMessage());
  }
  return getBasename();
}
