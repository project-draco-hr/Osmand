{
  return new StringBuffer("Openstreetmap Point ").append(this.getAction()).append(" ").append(this.getName()).append(" (").append(this.getId()).append("): [").append(this.getType()).append("/").append(this.getSubtype()).append(" (").append(this.getLatitude()).append(", ").append(this.getLongitude()).append(")]").toString();
}
