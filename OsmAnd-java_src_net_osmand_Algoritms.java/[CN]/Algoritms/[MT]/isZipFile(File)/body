{
  if (file.isDirectory()) {
    return false;
  }
  if (!file.canRead()) {
    throw new IOException("Cannot read file " + file.getAbsolutePath());
  }
  if (file.length() < 4) {
    return false;
  }
  DataInputStream in=new DataInputStream(new BufferedInputStream(new FileInputStream(file)));
  int test=in.readInt();
  in.close();
  return test == 0x504b0304;
}
