{
  final String query=data.getQuery();
  String value=null;
  if (query.contains(param)) {
    String[] params=query.split("&");
    for (    String p : params) {
      if (p.contains(param)) {
        value=p.substring(p.indexOf("=") + 1,p.length());
        break;
      }
    }
  }
  return value;
}
