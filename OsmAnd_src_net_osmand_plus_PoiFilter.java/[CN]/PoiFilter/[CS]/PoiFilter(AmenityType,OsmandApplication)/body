{
  this.application=application;
  isStandardFilter=true;
  filterId=STD_PREFIX + type;
  name=type == null ? application.getString(R.string.poi_filter_closest_poi) : OsmAndFormatter.toPublicString(type,application);
  if (type == null) {
    initSearchAll();
  }
 else {
    acceptedTypes.put(type,null);
  }
}
