{
  clearPreviousZoom();
  List<Amenity> amenityList=searchAmenities(lat,lon,matcher);
  MapUtils.sortListOfMapObject(amenityList,lat,lon);
  if (firstTimeLimit > 0) {
    while (amenityList.size() > firstTimeLimit) {
      amenityList.remove(amenityList.size() - 1);
    }
  }
  return amenityList;
}
