{
  Cursor cursor=getDatabase().rawQuery("SELECT image FROM tiles WHERE x = ? AND y = ? AND z = ?",new String[]{x + "",y + "",(17 - zoom) + ""});
  byte[] blob=null;
  if (cursor.moveToFirst()) {
    blob=cursor.getBlob(0);
  }
  cursor.close();
  if (blob != null) {
    return BitmapFactory.decodeByteArray(blob,0,blob.length);
  }
  return null;
}
