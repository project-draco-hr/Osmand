{
  if (isRoundAbout()) {
    return "Take " + getExitOut() + " exit";
  }
 else   if (value == C) {
    return "Go ahead";
  }
 else   if (value == TSLL) {
    return "Turn slightly left";
  }
 else   if (value == TL) {
    return "Turn left";
  }
 else   if (value == TSHL) {
    return "Turn sharply left";
  }
 else   if (value == TSLR) {
    return "Turn slightly right";
  }
 else   if (value == TR) {
    return "Turn right";
  }
 else   if (value == TSHR) {
    return "Turn sharply right";
  }
 else   if (value == TU) {
    return "Make uturn";
  }
 else   if (value == TRU) {
    return "Make uturn";
  }
 else   if (value == KL) {
    return "Keep left";
  }
 else   if (value == KR) {
    return "Keep right";
  }
 else   if (value == OFFR) {
    return "Off route";
  }
  return super.toString();
}
