{
  if (allTiles == null) {
    allTiles=new TIntArrayList(singleTiles);
    for (int k=0; k < boxTiles.size(); k+=4) {
      for (int x=boxTiles.get(k); x <= boxTiles.get(k + 2); x++) {
        for (int y=boxTiles.get(k + 1); y >= boxTiles.get(k + 3); y--) {
          allTiles.add(x);
          allTiles.add(y);
        }
      }
    }
  }
  return allTiles;
}
