{
  File extStorage=osmandSettings.extendOsmandPath(ResourceManager.VOICE_PATH);
  Set<String> setFiles=new LinkedHashSet<String>();
  if (extStorage.exists()) {
    for (    File f : extStorage.listFiles()) {
      if (f.isDirectory()) {
        setFiles.add(f.getName());
      }
    }
  }
  return setFiles;
}
