{
  TIntObjectHashMap<String> map=obj.getObjectNames();
  if (map != null) {
    map.forEachEntry(new TIntObjectProcedure<String>(){
      @Override public boolean execute(      int tag,      String name){
        if (name != null && name.trim().length() > 0) {
          createTextDrawInfo(obj,render,rc,pair,xMid,yMid,path,points,name,tag == obj.getMapIndex().nameEncodingType ? "" : obj.getMapIndex().decodeType(tag).tag);
        }
        return true;
      }
    }
);
  }
}
