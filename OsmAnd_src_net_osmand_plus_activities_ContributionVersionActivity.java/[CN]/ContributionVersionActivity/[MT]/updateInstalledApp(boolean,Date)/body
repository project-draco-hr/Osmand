{
  if (showMessage) {
    AccessibleToast.makeText(this,MessageFormat.format(getString(R.string.build_installed),currentSelectedBuild.tag,dateFormat.format(currentSelectedBuild.date)),Toast.LENGTH_LONG).show();
  }
  ((OsmandApplication)getApplicationContext()).getSettings().CONTRIBUTION_INSTALL_APP_DATE.set(dateFormat.format(d));
}
