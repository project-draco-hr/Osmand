{
  if (showMessage) {
    AccessibleToast.makeText(this,MessageFormat.format(getString(R.string.build_installed),currentSelectedBuild.tag,AndroidUtils.formatDate(getMyApplication(),currentSelectedBuild.date.getTime())),Toast.LENGTH_LONG).show();
  }
  getMyApplication().getSettings().CONTRIBUTION_INSTALL_APP_DATE.set(dateFormat.format(d));
}
