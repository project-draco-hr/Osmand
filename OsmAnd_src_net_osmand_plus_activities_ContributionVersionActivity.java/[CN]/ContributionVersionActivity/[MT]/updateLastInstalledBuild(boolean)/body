{
  if (currentSelectedBuild != null) {
    if (showMessage) {
      Toast.makeText(this,MessageFormat.format(getString(R.string.build_installed),currentSelectedBuild.tag,dateFormat.format(currentSelectedBuild.date)),Toast.LENGTH_LONG).show();
    }
    OsmandSettings.getPrefs(this).edit().putString(CONTRIBUTION_INSTALL_APP_DATE,dateFormat.format(currentSelectedBuild.date)).commit();
    currentInstalledDate=currentSelectedBuild.date;
    getListAdapter().notifyDataSetInvalidated();
  }
}
