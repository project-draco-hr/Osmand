{
  final java.text.Collator instance=Locale.getDefault().getLanguage().equals("ro") || Locale.getDefault().getLanguage().equals("cs") || Locale.getDefault().getLanguage().equals("sk") ? java.text.Collator.getInstance(Locale.US) : java.text.Collator.getInstance();
  instance.setStrength(java.text.Collator.PRIMARY);
  return wrapCollator(instance);
}
