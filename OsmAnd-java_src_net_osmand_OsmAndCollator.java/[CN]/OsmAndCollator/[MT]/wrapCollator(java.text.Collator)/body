{
  return new net.osmand.Collator(){
    @Override public int compare(    Object o1,    Object o2){
      return instance.compare(o1,o2);
    }
    @Override public boolean equals(    Object obj){
      return instance.equals(obj);
    }
    @Override public boolean equals(    String source,    String target){
      return instance.equals(source,target);
    }
    @Override public int compare(    String source,    String target){
      return instance.compare(source,target);
    }
  }
;
}
