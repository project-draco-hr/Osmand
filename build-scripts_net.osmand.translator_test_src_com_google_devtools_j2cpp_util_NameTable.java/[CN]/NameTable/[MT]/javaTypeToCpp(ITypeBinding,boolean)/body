{
  if (binding.isInterface() && !includeInterfaces || binding == Types.resolveIOSType("id") || binding == Types.resolveIOSType("NSObject")) {
    return NameTable.ID_TYPE;
  }
  if (binding.isTypeVariable()) {
    binding=binding.getErasure();
    if (Types.isJavaObjectType(binding) || binding.isInterface()) {
      return NameTable.ID_TYPE;
    }
  }
  return getFullName(binding);
}
