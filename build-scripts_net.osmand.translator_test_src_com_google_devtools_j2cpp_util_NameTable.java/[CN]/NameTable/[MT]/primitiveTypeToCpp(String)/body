{
  if (javaName.equals("boolean")) {
    return "bool";
  }
  if (javaName.equals("byte")) {
    return "signed short";
  }
  if (javaName.equals("char")) {
    return "wchar_t";
  }
  if (javaName.equals("short")) {
    return "signed short";
  }
  if (javaName.equals("long")) {
    return "signed long";
  }
  if (javaName.equals("float")) {
    return "float";
  }
  if (javaName.equals("double")) {
    return "double";
  }
  return javaName;
}
