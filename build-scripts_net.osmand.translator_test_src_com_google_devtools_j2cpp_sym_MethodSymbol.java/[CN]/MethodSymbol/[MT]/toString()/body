{
  StringBuilder sb=new StringBuilder(getName());
  sb.append('(');
  if (parameters != null) {
    Iterator<VariableSymbol> iter=parameters.iterator();
    while (iter.hasNext()) {
      sb.append(iter.next());
      if (iter.hasNext()) {
        sb.append(", ");
      }
    }
  }
  sb.append(')');
  return sb.toString();
}
