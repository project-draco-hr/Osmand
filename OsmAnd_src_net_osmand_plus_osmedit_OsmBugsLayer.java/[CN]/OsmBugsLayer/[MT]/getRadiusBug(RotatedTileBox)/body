{
  int z;
  final int zoom=tb.getZoom();
  if (zoom < startZoom) {
    z=0;
  }
 else   if (zoom <= 12) {
    z=8;
  }
 else   if (zoom <= 15) {
    z=10;
  }
 else   if (zoom == 16) {
    z=13;
  }
 else   if (zoom == 17) {
    z=15;
  }
 else {
    z=16;
  }
  return (int)(z * tb.getDensity());
}
