{
  if (comments.size() > 0) {
    StringBuilder sb=new StringBuilder();
    if (dates.size() > 0) {
      sb.append(dates.get(0)).append(" ");
    }
    if (users.size() > 0) {
      sb.append(users.get(0));
    }
    description=comments.get(0);
    typeName=sb.toString();
  }
  if (description != null) {
    if (comments.size() > 0) {
      comments.remove(0);
    }
    if (dates.size() > 0) {
      dates.remove(0);
    }
    if (users.size() > 0) {
      users.remove(0);
    }
  }
}
