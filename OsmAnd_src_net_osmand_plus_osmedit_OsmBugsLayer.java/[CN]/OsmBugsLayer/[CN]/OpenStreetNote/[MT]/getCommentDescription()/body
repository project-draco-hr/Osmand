{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < comments.size(); i++) {
    if (i < dates.size()) {
      sb.append(dates.get(i)).append(" ");
    }
    if (i < users.size()) {
      sb.append(users.get(i)).append(" : ");
    }
    sb.append(comments.get(i)).append("\n");
  }
  return sb.toString();
}
