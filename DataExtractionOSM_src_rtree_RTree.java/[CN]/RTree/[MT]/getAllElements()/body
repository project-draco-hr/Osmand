{
  fileHdr.lockRead();
  long root;
  root=fileHdr.getRootIndex();
  try {
    return (trvsRPost(chdNodes.getReadNode(fileHdr.getFile(),fileName,root,fileHdr),false));
  }
 catch (  Exception e) {
    throw new RTreeException("RTree.getAllElements: " + e.getMessage());
  }
 finally {
    fileHdr.unlock();
  }
}
