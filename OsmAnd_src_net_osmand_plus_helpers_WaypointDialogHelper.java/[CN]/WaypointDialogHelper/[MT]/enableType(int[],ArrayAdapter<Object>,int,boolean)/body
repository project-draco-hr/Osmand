{
  new AsyncTask<Void,Void,Void>(){
    @Override protected Void doInBackground(    Void... params){
      app.getWaypointHelper().enableWaypointType(type,enable);
      return null;
    }
    protected void onPostExecute(    Void result){
      running[0]=-1;
      reloadListAdapter(listAdapter);
    }
  }
.execute((Void)null);
}
