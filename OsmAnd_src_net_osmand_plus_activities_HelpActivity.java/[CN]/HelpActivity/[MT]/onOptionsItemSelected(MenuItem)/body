{
  int itemId=item.getItemId();
switch (itemId) {
case android.R.id.home:
    finish();
  return true;
case HOME:
wv.loadUrl(FILE_ANDROID_ASSET_HELP + "index.html");
return true;
case BACK:
if (wv.canGoBack()) {
wv.goBack();
}
return true;
case FORWARD:
if (wv.canGoForward()) {
wv.goForward();
}
return true;
case CLOSE:
finish();
return true;
}
return false;
}
