{
  createMenuItem(menu,HOME,R.string.home,R.drawable.ic_action_home_dark,R.drawable.ic_action_home_dark,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  createMenuItem(menu,BACK,R.string.previous_button,0,0,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  createMenuItem(menu,FORWARD,R.string.next_button,0,0,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  createMenuItem(menu,CLOSE,R.string.close,R.drawable.ic_action_ok_dark,R.drawable.ic_action_ok_dark,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  return super.onCreateOptionsMenu(menu);
}
