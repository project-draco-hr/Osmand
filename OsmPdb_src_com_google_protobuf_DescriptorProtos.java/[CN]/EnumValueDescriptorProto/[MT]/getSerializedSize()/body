{
  int size=memoizedSerializedSize;
  if (size != -1)   return size;
  size=0;
  if (hasName()) {
    size+=com.google.protobuf.CodedOutputStream.computeStringSize(1,getName());
  }
  if (hasNumber()) {
    size+=com.google.protobuf.CodedOutputStream.computeInt32Size(2,getNumber());
  }
  if (hasOptions()) {
    size+=com.google.protobuf.CodedOutputStream.computeMessageSize(3,getOptions());
  }
  size+=getUnknownFields().getSerializedSize();
  memoizedSerializedSize=size;
  return size;
}
