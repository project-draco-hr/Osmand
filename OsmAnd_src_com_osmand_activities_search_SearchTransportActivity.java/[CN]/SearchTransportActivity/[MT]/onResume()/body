{
  super.onResume();
  if (!Algoritms.objectEquals(OsmandSettings.getLastKnownMapLocation(this),this.lastKnownMapLocation) || !Algoritms.objectEquals(OsmandSettings.getPointToNavigate(this),this.destinationLocation)) {
    lastKnownMapLocation=OsmandSettings.getLastKnownMapLocation(this);
    destinationLocation=OsmandSettings.getPointToNavigate(this);
    searchTransport();
  }
}
