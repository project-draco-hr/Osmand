{
  MenuController menuController=getMenuController();
  if (menuController != null && menuController.needTypeStr()) {
    return typeStr;
  }
 else {
    if (Algorithms.isEmpty(streetStr)) {
      return PointDescription.getLocationName(getMapActivity(),getLatLon().getLatitude(),getLatLon().getLongitude(),true).replaceAll("\n","");
    }
 else {
      return streetStr;
    }
  }
}
