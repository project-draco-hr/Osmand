{
  addressNotKnownStr=getMapActivity().getString(R.string.looking_up_address) + getMapActivity().getString(R.string.shared_string_ellipsis);
  acquireIcons();
  acquireNameAndType();
  if (needStreetName()) {
    acquireStreetName();
  }
 else   if (nameStr.equals(addressNotKnownStr)) {
    nameStr="";
  }
}
