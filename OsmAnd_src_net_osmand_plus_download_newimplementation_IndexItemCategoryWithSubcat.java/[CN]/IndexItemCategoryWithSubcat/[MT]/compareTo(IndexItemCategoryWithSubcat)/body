{
  final int BEFORE=-1;
  final int EQUAL=0;
  final int AFTER=1;
  if (this == another)   return EQUAL;
  if (this.categoryStaticData.getOrder() < another.categoryStaticData.getOrder())   return BEFORE;
  if (this.categoryStaticData.getOrder() > another.categoryStaticData.getOrder())   return AFTER;
  return EQUAL;
}
