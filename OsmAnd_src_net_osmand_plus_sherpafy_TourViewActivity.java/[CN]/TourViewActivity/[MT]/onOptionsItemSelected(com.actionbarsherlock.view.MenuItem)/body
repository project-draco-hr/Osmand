{
  if (item.getItemId() == GO_TO_MAP) {
    goToMap();
    return true;
  }
 else   if (item.getItemId() == SETTINGS_ID) {
    startSettings();
    return true;
  }
 else   if (item.getItemId() == TOUR_ID) {
    startTourView();
    return true;
  }
 else {
    return super.onOptionsItemSelected(item);
  }
}
