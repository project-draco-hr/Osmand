{
  if (state == STATE_TOUR_VIEW) {
  }
 else   if (state == STATE_SELECT_TOUR) {
    if (customization.isTourSelected()) {
      createMenuItem(menu,ACTION_TOUR_ID,R.string.default_buttons_ok,R.drawable.ic_action_ok_light,R.drawable.ic_action_ok_dark,MenuItem.SHOW_AS_ACTION_IF_ROOM | MenuItem.SHOW_AS_ACTION_WITH_TEXT);
    }
  }
  return super.onCreateOptionsMenu(menu);
}
