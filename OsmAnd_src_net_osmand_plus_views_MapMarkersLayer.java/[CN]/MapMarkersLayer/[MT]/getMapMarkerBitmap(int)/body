{
switch (colorIndex) {
case 0:
    return markerBitmapBlue;
case 1:
  return markerBitmapGreen;
case 2:
return markerBitmapOrange;
case 3:
return markerBitmapRed;
case 4:
return markerBitmapYellow;
case 5:
return markerBitmapTeal;
case 6:
return markerBitmapPurple;
default :
return markerBitmapBlue;
}
}
