{
switch (colorIndex) {
case 0:
    return bitmapPaintDestBlue;
case 1:
  return bitmapPaintDestGreen;
case 2:
return bitmapPaintDestOrange;
case 3:
return bitmapPaintDestRed;
case 4:
return bitmapPaintDestYellow;
case 5:
return bitmapPaintDestTeal;
case 6:
return bitmapPaintDestPurple;
default :
return bitmapPaintDestBlue;
}
}
