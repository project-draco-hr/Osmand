{
  this.id=e.getId();
  this.type=EntityType.valueOf(e);
  if (this.name == null) {
    this.name=e.getTag(OSMTagKey.NAME);
  }
  if (this.enName == null) {
    this.enName=e.getTag(OSMTagKey.NAME_EN);
    if (name == null) {
      this.name=this.enName;
    }
  }
  if (this.location == null) {
    this.location=MapUtils.getCenter(e);
  }
}
