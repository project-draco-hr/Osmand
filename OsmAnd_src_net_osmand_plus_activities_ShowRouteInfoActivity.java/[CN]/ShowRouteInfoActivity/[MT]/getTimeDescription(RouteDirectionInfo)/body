{
  int seconds=model.getExpectedTime() % 60;
  int min=(model.getExpectedTime() / 60) % 60;
  int hours=(model.getExpectedTime() / 3600);
  String timeText;
  if (hours == 0) {
    timeText=String.format("%02d:%02d",min,seconds);
  }
 else {
    timeText=String.format("%d:%02d:%02d",hours,min,seconds);
  }
  return timeText;
}
