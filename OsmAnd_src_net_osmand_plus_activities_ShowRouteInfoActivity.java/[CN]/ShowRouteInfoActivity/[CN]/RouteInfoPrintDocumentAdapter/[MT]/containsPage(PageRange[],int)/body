{
  boolean contains=false;
  for (int i=0; i < pages.length; i++) {
    if (pages[i].equals(PageRange.ALL_PAGES)) {
      contains=true;
      break;
    }
 else     if ((pages[i].getStart() <= page) && (page <= pages[i].getEnd())) {
      contains=true;
      break;
    }
  }
  return contains;
}
