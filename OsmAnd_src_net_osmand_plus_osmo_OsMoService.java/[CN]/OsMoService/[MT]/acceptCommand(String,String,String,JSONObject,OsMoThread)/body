{
  if (command.equals("MOTD")) {
    SessionInfo si=getCurrentSessionInfo();
    if (si != null) {
      if (data.startsWith("[")) {
        try {
          data=new JSONArray(data).getString(0);
        }
 catch (        JSONException e) {
          e.printStackTrace();
        }
      }
      si.motd=data;
    }
    return true;
  }
  return false;
}
