{
  String name=en ? street.getEnName() : street.getName();
  name=name.toLowerCase();
  if (!Algorithms.isEmpty(name)) {
    if (!streets.containsKey(name)) {
      return streets.put(name,street);
    }
 else {
      Street prev=streets.get(name);
      prev.mergeWith(street);
      return prev;
    }
  }
  return null;
}
