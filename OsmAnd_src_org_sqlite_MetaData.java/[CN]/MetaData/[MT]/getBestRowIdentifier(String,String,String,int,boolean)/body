{
  if (getBestRowIdentifier == null)   getBestRowIdentifier=conn.prepareStatement("select " + "null as SCOPE, " + "null as COLUMN_NAME, "+ "null as DATA_TYPE, "+ "null as TYPE_NAME, "+ "null as COLUMN_SIZE, "+ "null as BUFFER_LENGTH, "+ "null as DECIMAL_DIGITS, "+ "null as PSEUDO_COLUMN limit 0;");
  return getBestRowIdentifier.executeQuery();
}
