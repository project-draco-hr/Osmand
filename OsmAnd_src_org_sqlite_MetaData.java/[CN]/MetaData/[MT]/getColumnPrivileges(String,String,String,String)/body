{
  if (getColumnPrivileges == null)   getColumnPrivileges=conn.prepareStatement("select " + "null as TABLE_CAT, " + "null as TABLE_SCHEM, "+ "null as TABLE_NAME, "+ "null as COLUMN_NAME, "+ "null as GRANTOR, "+ "null as GRANTEE, "+ "null as PRIVILEGE, "+ "null as IS_GRANTABLE limit 0;");
  return getColumnPrivileges.executeQuery();
}
