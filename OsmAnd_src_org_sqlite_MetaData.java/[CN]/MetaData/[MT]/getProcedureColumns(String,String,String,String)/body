{
  if (getProcedures == null)   getProcedureColumns=conn.prepareStatement("select " + "null as PROCEDURE_CAT, " + "null as PROCEDURE_SCHEM, "+ "null as PROCEDURE_NAME, "+ "null as COLUMN_NAME, "+ "null as COLUMN_TYPE, "+ "null as DATA_TYPE, "+ "null as TYPE_NAME, "+ "null as PRECISION, "+ "null as LENGTH, "+ "null as SCALE, "+ "null as RADIX, "+ "null as NULLABLE, "+ "null as REMARKS limit 0;");
  return getProcedureColumns.executeQuery();
}
