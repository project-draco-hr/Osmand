{
  if (getProcedures == null)   getProcedures=conn.prepareStatement("select " + "null as PROCEDURE_CAT, " + "null as PROCEDURE_SCHEM, "+ "null as PROCEDURE_NAME, "+ "null as UNDEF1, "+ "null as UNDEF2, "+ "null as UNDEF3, "+ "null as REMARKS, "+ "null as PROCEDURE_TYPE limit 0;");
  return getProcedures.executeQuery();
}
