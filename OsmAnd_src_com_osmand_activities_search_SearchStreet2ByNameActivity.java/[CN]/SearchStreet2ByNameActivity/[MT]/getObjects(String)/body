{
  int ind=0;
  filter=filter.toLowerCase();
  if (filter.length() == 0) {
    return initialList;
  }
  filterList.clear();
  for (  Street s : initialList) {
    String lowerCase=s.getName(region.useEnglishNames()).toLowerCase();
    if (lowerCase.startsWith(filter)) {
      filterList.add(ind,s);
      ind++;
    }
 else     if (lowerCase.contains(filter)) {
      filterList.add(s);
    }
  }
  return filterList;
}
