{
  LatLon l=node.getLatLon();
  double lon=l.getLongitude();
  if (180 - Math.abs(l.getLongitude()) < 0.0001) {
    if (l.getLongitude() < 0) {
      lon=-179.9999;
    }
 else {
      lon=180;
    }
  }
  return ((long)MapUtils.getTileNumberY(21,l.getLatitude()) << 32l) + ((long)MapUtils.getTileNumberX(21,lon));
}
