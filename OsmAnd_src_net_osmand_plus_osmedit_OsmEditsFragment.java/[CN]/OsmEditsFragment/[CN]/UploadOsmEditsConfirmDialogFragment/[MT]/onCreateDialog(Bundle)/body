{
  final OsmPoint[] points=(OsmPoint[])getArguments().getSerializable(POINTS_ARRAY);
  AlertDialog.Builder builder=new AlertDialog.Builder(getActivity());
  builder.setMessage(getString(R.string.local_osm_changes_upload_all_confirm,points.length));
  builder.setPositiveButton(R.string.shared_string_yes,new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int which){
      ((OsmEditsFragment)getParentFragment()).showUploadItemsProgressDialog(points);
    }
  }
);
  builder.setNegativeButton(R.string.shared_string_cancel,null);
  return builder.create();
}
