{
  if (warnings != null && !warnings.isEmpty()) {
    final StringBuilder b=new StringBuilder();
    boolean f=true;
    for (    String w : warnings) {
      if (f) {
        f=false;
      }
 else {
        b.append('\n');
      }
      b.append(w);
    }
    showWarning(uiContext,b.toString());
  }
}
