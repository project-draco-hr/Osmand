{
  int oldSize=0;
  while (getOuterWays().size() != oldSize) {
    oldSize=getOuterWays().size();
    mergeOuterWays();
  }
  return getOuterWays().size() == 1 && getOuterWays().get(0).getNodes().get(0).getId() == getOuterWays().get(0).getNodes().get(getOuterWays().get(0).getNodes().size() - 1).getId();
}
