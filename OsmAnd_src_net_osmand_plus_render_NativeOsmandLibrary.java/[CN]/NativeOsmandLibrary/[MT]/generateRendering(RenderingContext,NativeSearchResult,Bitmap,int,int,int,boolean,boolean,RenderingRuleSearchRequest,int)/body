{
  if (searchResultHandler == null) {
    log.error("Error searchresult = null");
    return new RenderingGenerationResult(null);
  }
  if (android.os.Build.VERSION.SDK_INT >= 8)   return generateRendering_Direct(rc,searchResultHandler.nativeHandler,bitmap,useEnglishNames,render,defaultColor);
 else   return generateRendering_Indirect(rc,searchResultHandler.nativeHandler,requestedBitmapWidth,requestedBitmapHeight,rowBytes,isTransparent,useEnglishNames,render,defaultColor);
}
