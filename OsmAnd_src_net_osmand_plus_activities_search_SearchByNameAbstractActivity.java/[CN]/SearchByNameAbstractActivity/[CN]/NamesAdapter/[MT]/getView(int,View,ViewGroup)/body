{
  View row;
  if (convertView != null) {
    row=convertView;
  }
 else {
    LayoutInflater inflater=getLayoutInflater();
    row=inflater.inflate(R.layout.searchbyname_list,parent,false);
  }
  TextView label=(TextView)row.findViewById(R.id.NameLabel);
  label.setText(getText(getItem(position)));
  return row;
}
