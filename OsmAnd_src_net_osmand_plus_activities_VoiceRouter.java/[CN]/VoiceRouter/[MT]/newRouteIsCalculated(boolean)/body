{
  CommandBuilder play=getNewCommandPlayerToPlay();
  if (play != null) {
    if (updateRoute) {
      play.routeRecalculated(router.getLeftDistance()).play();
    }
 else {
      play.newRouteCalculated(router.getLeftDistance()).play();
    }
  }
  currentDirection=router.currentDirectionInfo;
  currentStatus=0;
}
