{
  if (nodeIds != null) {
    if (nodes == null) {
      nodes=new ArrayList<Node>();
    }
 else {
      nodes.clear();
    }
    for (int i=0; i < nodeIds.size(); i++) {
      if (entities.get(nodeIds.get(i)) instanceof Node) {
        nodes.add((Node)entities.get(nodeIds.get(i)));
      }
 else {
        nodes.add(null);
      }
    }
  }
}
