{
  if (!Algoritms.objectEquals(currentTask,taskName)) {
    this.currentTask=taskName;
    log.info("Memory before task exec: " + Runtime.getRuntime().totalMemory() + " free : "+ Runtime.getRuntime().freeMemory());
    if (previousTaskStarted == 0) {
      log.info(taskName + " started - " + work);
    }
 else {
      log.info(taskName + " started after " + (System.currentTimeMillis() - previousTaskStarted)+ " ms"+ " - "+ work);
    }
    previousTaskStarted=System.currentTimeMillis();
  }
  startWork(work);
}
