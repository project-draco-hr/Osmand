{
  check.setOnClickListener(new View.OnClickListener(){
    @Override public void onClick(    View v){
      if (check.isChecked()) {
        filter.setTypeToAccept(category,true);
        showDialog(category);
      }
 else {
        filter.setTypeToAccept(category,false);
        saveFilter();
      }
      notifyDataSetChanged();
    }
  }
);
}
