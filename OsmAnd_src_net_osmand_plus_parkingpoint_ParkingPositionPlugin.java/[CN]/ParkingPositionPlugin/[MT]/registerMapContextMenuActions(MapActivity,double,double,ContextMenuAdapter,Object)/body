{
  ItemClickListener addListener=new ItemClickListener(){
    @Override public boolean onContextMenuClick(    ArrayAdapter<?> adapter,    int resId,    int pos,    boolean isChecked){
      if (resId == R.string.context_menu_item_add_parking_point) {
        showAddParkingDialog(mapActivity,latitude,longitude);
      }
      return true;
    }
  }
;
  adapter.addItem(new ContextMenuItem.ItemBuilder().setTitleId(R.string.context_menu_item_add_parking_point,mapActivity).setColorIcon(R.drawable.ic_action_parking_dark).setListener(addListener).createItem());
}
