{
  if (parkingLayer != null) {
    if (getParkingPosition() != null) {
      boolean l=app.getSettings().isLightContent();
      helper.registerItem(R.string.osmand_parking_delete,l ? R.drawable.a_1_navigation_cancel_light : R.drawable.a_1_navigation_cancel_dark,new OnContextMenuClick(){
        @Override public void onContextMenuClick(        int itemId,        int pos,        boolean isChecked,        DialogInterface dialog){
          showDeleteDialog(mapActivity);
        }
      }
);
    }
  }
}
