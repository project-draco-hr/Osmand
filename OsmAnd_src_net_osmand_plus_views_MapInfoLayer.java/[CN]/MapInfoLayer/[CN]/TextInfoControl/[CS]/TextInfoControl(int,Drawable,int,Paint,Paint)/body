{
  super(background);
  this.leftMargin=leftMargin;
  this.imageDrawable=drawable;
  if (imageDrawable != null) {
    imageDrawable.setBounds(0,(int)(3 * scaleCoefficient),imageDrawable.getMinimumWidth(),imageDrawable.getMinimumHeight() + (int)(3 * scaleCoefficient));
  }
  this.textPaint=textPaint;
  this.subtextPaint=subtextPaint;
}
