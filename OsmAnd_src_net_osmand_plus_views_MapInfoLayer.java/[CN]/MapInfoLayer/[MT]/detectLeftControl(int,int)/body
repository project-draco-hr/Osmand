{
  x-=LEFT_MARGIN;
  if (y >= 0 && x >= 0) {
    for (int i=0; i < leftControls.size(); i++) {
      if (leftControls.get(i).isVisible()) {
        y-=leftControls.get(i).getHeight();
        if (y <= 0) {
          if (x <= leftControls.get(i).getWidth()) {
            return leftControls.get(i);
          }
 else {
            return null;
          }
        }
      }
    }
  }
  return null;
}
