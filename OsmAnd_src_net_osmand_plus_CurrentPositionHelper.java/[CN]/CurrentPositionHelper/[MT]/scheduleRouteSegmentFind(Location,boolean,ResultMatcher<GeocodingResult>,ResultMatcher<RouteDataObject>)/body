{
  boolean res=false;
  if (loc != null) {
    new AsyncTask<Void,Void,Void>(){
      @Override protected Void doInBackground(      Void... params){
        try {
          processGeocoding(loc,geoCoding,storeFound,result);
        }
 catch (        Exception e) {
          e.printStackTrace();
        }
        return null;
      }
    }
.execute((Void)null);
    res=true;
  }
  return res;
}
