{
  RoutePlannerFrontEnd rp=new RoutePlannerFrontEnd(false);
  if (ctx == null || am != app.getSettings().getApplicationMode()) {
    initCtx(app);
    if (ctx == null) {
      return null;
    }
  }
  RouteSegment sg=rp.findRouteSegment(lat,lon,ctx);
  if (sg == null) {
    return null;
  }
  return sg.getRoad();
}
