{
  if (ctx == null || mapsIndexed || am != app.getSettings().getApplicationMode()) {
    initCtx(app);
    mapsIndexed=false;
    if (ctx == null) {
      return null;
    }
  }
  try {
    return new GeocodingUtilities().reverseGeocodingSearch(geocoding ? defCtx : ctx,lat,lon);
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
