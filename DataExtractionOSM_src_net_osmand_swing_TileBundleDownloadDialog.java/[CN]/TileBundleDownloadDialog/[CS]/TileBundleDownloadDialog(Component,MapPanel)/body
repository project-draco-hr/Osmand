{
  super(JOptionPane.getFrameForComponent(parent),true);
  map=panel.getMap();
  panel.getLatitude();
  selectionArea=panel.getSelectionArea();
  zoom=panel.getZoom();
  tilesLocation=panel.getTilesLocation();
