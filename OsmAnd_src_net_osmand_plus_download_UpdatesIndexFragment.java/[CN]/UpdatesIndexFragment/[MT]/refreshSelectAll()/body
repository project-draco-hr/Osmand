{
  View view=getView();
  if (view == null) {
    return;
  }
  CheckBox selectAll=(CheckBox)view.findViewById(R.id.select_all);
  for (  IndexItem item : indexItems) {
    if (!getMyActivity().getEntriesToDownload().containsKey(item)) {
      selectAll.setChecked(false);
      return;
    }
  }
  selectAll.setChecked(true);
}
