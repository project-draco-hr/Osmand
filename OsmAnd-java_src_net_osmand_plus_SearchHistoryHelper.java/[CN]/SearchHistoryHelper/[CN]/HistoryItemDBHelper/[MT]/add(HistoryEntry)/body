{
  SQLiteConnection db=openConnection(false);
  if (db != null) {
    try {
      db.execSQL("INSERT INTO " + HISTORY_TABLE_NAME + " VALUES (?, ?, ?, ?, ?)",new Object[]{e.getName(),System.currentTimeMillis(),null,e.getLat(),e.getLon()});
    }
  finally {
      db.close();
    }
    return true;
  }
  return false;
}
