{
  if (result != null) {
    if (result.booleanValue() && runOnSuccess != null) {
      runOnSuccess.run();
    }
 else     if (!result.booleanValue()) {
      Toast.makeText(ctx,R.string.input_output_error,Toast.LENGTH_LONG).show();
    }
  }
  if (progress.getDialog().isShowing()) {
    progress.getDialog().dismiss();
  }
}
