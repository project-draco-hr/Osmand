{
  if (chainNodes == null) {
    chainNodes=new ArrayList<Node>();
    for (int j=0; j < ways.size(); j++) {
      List<Node> ns=ways.get(j).getNodes();
      if (j == 0 && ns.get(0) != null) {
        chainNodes.add(ns.get(0));
      }
      for (int i=1; i < ns.size(); i++) {
        if (ns.get(i) != null) {
          chainNodes.add(ns.get(i));
        }
      }
    }
  }
  return chainNodes;
}
