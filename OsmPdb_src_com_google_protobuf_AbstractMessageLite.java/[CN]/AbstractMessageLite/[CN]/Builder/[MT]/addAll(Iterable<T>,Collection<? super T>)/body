{
  for (  final T value : values) {
    if (value == null) {
      throw new NullPointerException();
    }
  }
  if (values instanceof Collection) {
    final Collection<T> collection=(Collection<T>)values;
    list.addAll(collection);
  }
 else {
    for (    final T value : values) {
      list.add(value);
    }
  }
}
