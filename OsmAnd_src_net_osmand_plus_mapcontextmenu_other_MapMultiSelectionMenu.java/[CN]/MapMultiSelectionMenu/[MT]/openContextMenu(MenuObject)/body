{
  if (selectedObjects.containsKey(menuObject.getObject())) {
    selectedObjects.remove(menuObject.getObject());
  }
  hide();
  Object o=menuObject.getObject();
  getMapActivity().getMapLayers().getContextMenuLayer().tryInitSelectedObjectContextMenuProvider(o);
  getMapActivity().getMapViewTrackingUtilities().locationChanged(menuObject.getLatLon().getLatitude(),menuObject.getLatLon().getLongitude(),this);
  getMapActivity().getContextMenu().show(menuObject.getLatLon(),menuObject.getPointDescription(),menuObject.getObject());
}
