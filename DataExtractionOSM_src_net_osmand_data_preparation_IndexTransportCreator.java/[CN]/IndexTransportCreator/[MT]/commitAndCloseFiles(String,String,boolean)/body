{
  if (transportStopsTree != null) {
    transportStopsTree.getFileHdr().getFile().close();
    File f=new File(rtreeStopsFileName);
    if (f.exists() && deleteDatabaseIndexes) {
      f.delete();
    }
    f=new File(rtreeStopsPackFileName);
    if (f.exists() && deleteDatabaseIndexes) {
      f.delete();
    }
  }
}
