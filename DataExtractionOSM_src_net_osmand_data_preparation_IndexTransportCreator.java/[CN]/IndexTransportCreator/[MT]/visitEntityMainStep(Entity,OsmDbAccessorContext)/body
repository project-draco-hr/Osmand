{
  if (e instanceof Relation && e.getTag(OSMTagKey.ROUTE) != null) {
    ctx.loadEntityData(e,true);
    TransportRoute route=indexTransportRoute((Relation)e);
    if (route != null) {
      insertTransportIntoIndex(transRouteStat,transRouteStopsStat,transStopsStat,transportStopsTree,visitedStops,route,pStatements,BATCH_SIZE);
    }
  }
}
