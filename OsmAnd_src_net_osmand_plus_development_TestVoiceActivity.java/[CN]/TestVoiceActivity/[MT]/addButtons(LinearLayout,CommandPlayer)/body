  addButton(ll,"Keep left '' then after 400m keep right 'A1'",builder(p).turn(AbstractPrologCommandPlayer.A_LEFT_KEEP,"").then().turn(AbstractPrologCommandPlayer.A_RIGHT_KEEP,400,"A1"));
