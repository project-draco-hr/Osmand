{
  if (matcher.matches(pc.getTranslation()) || matcher.matches(pc.getKeyName().replace('_',' '))) {
    tm.put(pc.getTranslation(),pc);
  }
  List<PoiType> additionals=pc.getPoiAdditionals();
  if (additionals != null) {
    for (    PoiType a : additionals) {
      addIf(tm,a,matcher);
    }
  }
}
