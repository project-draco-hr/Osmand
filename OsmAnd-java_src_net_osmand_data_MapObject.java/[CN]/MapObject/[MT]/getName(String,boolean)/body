{
  if (lang != null) {
    if (lang.equals("en")) {
      return getEnName(true);
    }
 else {
      if (names != null) {
        String nm=names.get(lang);
        if (!Algorithms.isEmpty(nm)) {
          return nm;
        }
        if (transliterate) {
          return Junidecode.unidecode(getName());
        }
      }
    }
  }
  return getName();
}
