{
  if (invocationArguments.size() != parameterTypes.length) {
    return false;
  }
  for (int i=0; i < parameterTypes.length; i++) {
    ITypeBinding argType=Types.getTypeBinding(invocationArguments.get(i));
    if (!parameterTypes[i].isEqualTo(argType)) {
      return false;
    }
  }
  return true;
}
