{
  if (from == null) {
    return;
  }
  if (from.showCurrentTrack) {
    to.showCurrentTrack=true;
  }
  if (from.points != null) {
    to.points.addAll(from.points);
  }
  if (from.tracks != null) {
    to.tracks.addAll(from.tracks);
  }
  if (from.routes != null) {
    to.routes.addAll(from.routes);
  }
  if (from.warning != null) {
    to.warning=from.warning;
  }
}
