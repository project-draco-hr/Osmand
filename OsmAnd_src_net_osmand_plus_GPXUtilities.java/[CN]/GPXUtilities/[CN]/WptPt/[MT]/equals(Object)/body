{
  if (this == obj)   return true;
  if (obj == null || getClass() != obj.getClass())   return false;
  WptPt other=(WptPt)obj;
  return Algorithms.objectEquals(other.name,name) && Algorithms.objectEquals(other.category,category) && Algorithms.objectEquals(other.lat,lat)&& Algorithms.objectEquals(other.lon,lon)&& Algorithms.objectEquals(other.desc,desc);
}
