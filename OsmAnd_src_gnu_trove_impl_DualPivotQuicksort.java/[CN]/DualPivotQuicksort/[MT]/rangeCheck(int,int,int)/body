{
  if (fromIndex > toIndex) {
    throw new IllegalArgumentException("fromIndex(" + fromIndex + ") > toIndex("+ toIndex+ ")");
  }
  if (fromIndex < 0) {
    throw new ArrayIndexOutOfBoundsException(fromIndex);
  }
  if (toIndex > length) {
    throw new ArrayIndexOutOfBoundsException(toIndex);
  }
}
