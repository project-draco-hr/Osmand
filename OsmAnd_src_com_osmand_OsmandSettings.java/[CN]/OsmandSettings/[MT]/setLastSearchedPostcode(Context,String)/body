{
  SharedPreferences prefs=ctx.getSharedPreferences(SHARED_PREFERENCES_NAME,Context.MODE_WORLD_READABLE);
  Editor edit=prefs.edit().putLong(LAST_SEARCHED_CITY,-1).putString(LAST_SEARCHED_STREET,"").putString(LAST_SEARCHED_BUILDING,"").putString(lAST_SEARCHED_POSTCODE,postcode);
  if (prefs.contains(LAST_SEARCHED_INTERSECTED_STREET)) {
    edit.putString(LAST_SEARCHED_INTERSECTED_STREET,"");
  }
  return edit.commit();
}
