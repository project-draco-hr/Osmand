{
  try {
    if (elmt1 instanceof NonLeafElement || elmt2 instanceof NonLeafElement)     return elmt1.getRect().overlaps(elmt2.getRect());
 else {
      if (side == Join.LEFT)       return elmt1.getRect().contains(elmt2.getRect());
 else       return elmt2.getRect().contains(elmt1.getRect());
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    return false;
  }
}
