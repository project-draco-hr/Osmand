{
  Class<? extends LocationPoint> fcl=getFavoriteClass();
  if (o != null && fcl.isInstance(o)) {
    String desciption=((FavouritePoint)o).getDescription() != null ? " " + ((FavouritePoint)o).getDescription() : "";
    return PointDescription.getSimpleName((LocationPoint)o,view.getContext()) + " " + desciption;
  }
  return null;
}
