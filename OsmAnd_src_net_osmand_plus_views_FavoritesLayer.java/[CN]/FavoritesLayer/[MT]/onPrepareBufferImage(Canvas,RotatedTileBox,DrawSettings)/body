{
  if (tileBox.getZoom() >= startZoom) {
    final QuadRect latLonBounds=tileBox.getLatLonBounds();
    for (    FavouritePoint o : favorites.getFavouritePoints()) {
      if (o.getLatitude() >= latLonBounds.bottom && o.getLatitude() <= latLonBounds.top && o.getLongitude() >= latLonBounds.left && o.getLongitude() <= latLonBounds.right) {
        int x=(int)tileBox.getPixXFromLatLon(o.getLatitude(),o.getLongitude());
        int y=(int)tileBox.getPixYFromLatLon(o.getLatitude(),o.getLongitude());
        canvas.drawBitmap(favoriteIcon,x - favoriteIcon.getWidth() / 2,y - favoriteIcon.getHeight(),paint);
      }
    }
  }
}
