{
  if (Double.doubleToLongBits(displayDensityFactor) != Double.doubleToLongBits(other.displayDensityFactor))   return false;
  if (langId == null) {
    if (other.langId != null)     return false;
  }
 else   if (!langId.equals(other.langId))   return false;
  if (langPref != other.langPref)   return false;
  if (mapStyle == null) {
    if (other.mapStyle != null)     return false;
  }
 else   if (!mapStyle.equals(other.mapStyle))   return false;
  return true;
}
