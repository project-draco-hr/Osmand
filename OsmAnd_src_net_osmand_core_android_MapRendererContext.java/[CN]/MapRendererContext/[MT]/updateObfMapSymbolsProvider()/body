{
  if (_obfMapSymbolsProvider != null && _mapRendererView != null)   _mapRendererView.removeSymbolsProvider(_obfMapSymbolsProvider);
  _obfMapSymbolsProvider=new MapObjectsSymbolsProvider(_mapPrimitivesProvider,_referenceTileSize);
  if (_mapRendererView != null)   _mapRendererView.addSymbolsProvider(_obfMapSymbolsProvider);
}
