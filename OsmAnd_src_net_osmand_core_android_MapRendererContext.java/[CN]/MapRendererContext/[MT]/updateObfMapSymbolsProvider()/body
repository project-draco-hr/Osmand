{
  if (obfMapSymbolsProvider != null && mapRendererView != null)   mapRendererView.removeSymbolsProvider(obfMapSymbolsProvider);
  obfMapSymbolsProvider=new MapObjectsSymbolsProvider(mapPrimitivesProvider,referenceTileSizef);
  if (mapRendererView != null)   mapRendererView.addSymbolsProvider(obfMapSymbolsProvider);
}
