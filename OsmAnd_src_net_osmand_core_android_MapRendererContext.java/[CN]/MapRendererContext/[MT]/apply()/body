{
  if (mapRendererView instanceof AtlasMapRendererView)   ((AtlasMapRendererView)mapRendererView).setReferenceTileSizeOnScreenInPixels(referenceTileSizef);
  if (obfMapRasterLayer != null && obfMapRasterLayerProvider != null)   mapRendererView.setMapLayerProvider(obfMapRasterLayer,obfMapRasterLayerProvider);
  if (obfMapSymbolsProvider != null)   mapRendererView.addSymbolsProvider(obfMapSymbolsProvider);
}
