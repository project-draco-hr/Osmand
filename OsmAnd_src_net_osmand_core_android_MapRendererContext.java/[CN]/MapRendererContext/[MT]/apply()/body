{
  if (_mapRendererView instanceof AtlasMapRendererView)   ((AtlasMapRendererView)_mapRendererView).setReferenceTileSizeOnScreenInPixels(_referenceTileSize);
  if (_obfMapRasterLayer != null && _obfMapRasterLayerProvider != null)   _mapRendererView.setMapLayerProvider(_obfMapRasterLayer,_obfMapRasterLayerProvider);
  if (_obfMapSymbolsProvider != null)   _mapRendererView.addSymbolsProvider(_obfMapSymbolsProvider);
}
