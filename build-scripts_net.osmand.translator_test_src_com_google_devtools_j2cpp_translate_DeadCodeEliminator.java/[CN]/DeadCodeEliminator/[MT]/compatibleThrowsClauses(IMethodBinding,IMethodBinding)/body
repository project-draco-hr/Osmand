{
  for (  ITypeBinding type1 : overrider.getExceptionTypes()) {
    boolean present=false;
    for (    ITypeBinding type2 : overridee.getExceptionTypes()) {
      if (type1.isSubTypeCompatible(type2)) {
        present=true;
        break;
      }
    }
    if (!present) {
      return false;
    }
  }
  return true;
}
