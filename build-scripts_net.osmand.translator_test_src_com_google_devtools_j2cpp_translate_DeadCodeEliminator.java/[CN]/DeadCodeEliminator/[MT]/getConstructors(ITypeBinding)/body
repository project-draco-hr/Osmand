{
  final String classSignature=Types.getSignature(clazz);
  return Iterators.filter(Iterators.forArray(clazz.getDeclaredMethods()),new Predicate<IMethodBinding>(){
    @Override public boolean apply(    IMethodBinding method){
      return method.isConstructor() && !deadCodeMap.isDeadMethod(classSignature,getProGuardName(method),Types.getSignature(method));
    }
  }
);
}
