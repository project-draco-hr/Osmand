{
  if (overrideEquivalentMethods.size() == 1 && !Modifier.isAbstract(overrideEquivalentMethods.get(0).getModifiers())) {
    return overrideEquivalentMethods.get(0);
  }
  IMethodBinding concrete=null;
  for (  IMethodBinding method : overrideEquivalentMethods) {
    int modifiers=method.getModifiers();
    if (!Modifier.isAbstract(modifiers) && !Modifier.isNative(modifiers)) {
      if (concrete == null) {
        concrete=method;
      }
 else {
        throw new AssertionError("Can't inherit multiple concrete methods with same signature");
      }
    }
  }
  if (concrete == null) {
    return null;
  }
  for (  IMethodBinding inherited : overrideEquivalentMethods) {
    if (!isSubstitutableBy(concrete.getReturnType()).apply(inherited.getReturnType()) || !compatibleThrowsClauses(concrete,inherited)) {
      return null;
    }
  }
  return concrete;
}
