{
  return new Predicate<ITypeBinding>(){
    @Override public boolean apply(    ITypeBinding supertype){
      return subtype.isPrimitive() && subtype.isEqualTo(supertype) || subtype.isSubTypeCompatible(supertype) || subtype.isEqualTo(supertype.getErasure()) || subtype.getName().equals("void") && supertype.getName().equals("void");
    }
  }
;
}
