{
  String pr;
  if (segment.parentRoute != null) {
    pr=" pend=" + segment.parentSegmentEnd + " parent="+ segment.parentRoute.road;
  }
 else {
    pr="";
  }
  System.out.println("" + segment.road + " ind="+ segment.getSegmentStart()+ " ds="+ ((float)segment.distanceFromStart)+ " es="+ ((float)segment.distanceToEnd)+ pr);
}
