{
  android.database.sqlite.SQLiteDatabase db=SQLiteDatabase.openDatabase(app.getDatabasePath(name).getAbsolutePath(),null,(readOnly ? SQLiteDatabase.OPEN_READONLY : (SQLiteDatabase.OPEN_READWRITE | SQLiteDatabase.ENABLE_WRITE_AHEAD_LOGGING)) | SQLiteDatabase.CREATE_IF_NECESSARY);
  if (db == null) {
    return null;
  }
  return new SQLiteDatabaseWrapper(db);
}
