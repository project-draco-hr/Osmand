{
  List<Street> streets=new ArrayList<Street>();
  Statement stat=c.createStatement();
  ResultSet set=stat.executeQuery(IndexConstants.generateSelectSQL(IndexStreetTable.values(),IndexStreetTable.CITY.toString() + " = " + city.getId()));
  while (set.next()) {
    Street street=new Street(city);
    street.setName(set.getString(IndexStreetTable.NAME.ordinal() + 1));
    street.setEnName(set.getString(IndexStreetTable.NAME_EN.ordinal() + 1));
    street.setLocation(set.getDouble(IndexStreetTable.LATITUDE.ordinal() + 1),set.getDouble(IndexStreetTable.LONGITUDE.ordinal() + 1));
    street.setId(set.getLong(IndexStreetTable.ID.ordinal() + 1));
    streets.add(street);
  }
  set.close();
  stat.close();
  return streets;
}
