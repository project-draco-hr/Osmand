{
  SQLiteConnection db=openConnection(false);
  if (db != null) {
    try {
      db.execSQL("UPDATE " + HISTORY_TABLE_NAME + " SET time = ? WHERE "+ HISTORY_COL_NAME+ " = ?",new Object[]{System.currentTimeMillis(),e.getName()});
    }
  finally {
      db.close();
    }
    return true;
  }
  return false;
}
