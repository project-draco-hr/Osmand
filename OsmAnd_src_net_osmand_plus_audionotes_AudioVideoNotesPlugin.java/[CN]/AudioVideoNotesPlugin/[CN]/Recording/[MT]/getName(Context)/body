{
  String fileName=file.getName();
  String desc=getDescriptionName(fileName);
  if (desc != null) {
    return desc;
  }
 else   if (this.isAudio()) {
    return formatDateTime(ctx,file.lastModified());
  }
 else   if (this.isVideo()) {
    return formatDateTime(ctx,file.lastModified());
  }
 else   if (this.isPhoto()) {
    return formatDateTime(ctx,file.lastModified());
  }
  return "";
}
