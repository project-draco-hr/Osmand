{
  File avPath=app.getAppPath(IndexConstants.AV_INDEX_DIR);
  if (avPath.canRead()) {
    if (!reIndexAndKeepOld) {
      recordings.clear();
      recordingByFileName.clear();
    }
    File[] files=avPath.listFiles();
    if (files != null) {
      for (      File f : files) {
        indexFile(registerNew,f);
      }
    }
  }
  return null;
}
