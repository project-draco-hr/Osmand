{
  File avPath=app.getSettings().extendOsmandPath(ResourceManager.AV_PATH);
  if (avPath.canRead()) {
    recordings.clear();
    File[] files=avPath.listFiles();
    if (files != null) {
      for (      File f : files) {
        if (f.getName().endsWith(THREEGP_EXTENSION) || f.getName().endsWith(MPEG4_EXTENSION)) {
          indexFile(f);
        }
      }
    }
  }
  return null;
}
