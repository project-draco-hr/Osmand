{
  if (mIsStarted)   return;
  for (  ViewPropertyAnimatorCompat animator : mAnimators) {
    if (mDuration >= 0) {
      animator.setDuration(mDuration);
    }
    if (mInterpolator != null) {
      animator.setInterpolator(mInterpolator);
    }
    if (mListener != null) {
      animator.setListener(mProxyListener);
    }
    animator.start();
  }
  mIsStarted=true;
}
