{
  List<Amenity> amenities=new ArrayList<Amenity>();
  for (  AmenityIndexRepository index : amenityRepositories) {
    if (index.checkContains(topLatitude,leftLongitude,bottomLatitude,rightLongitude)) {
      index.searchAmenities(MapUtils.get31TileNumberY(topLatitude),MapUtils.get31TileNumberX(leftLongitude),MapUtils.get31TileNumberY(bottomLatitude),MapUtils.get31TileNumberX(rightLongitude),-1,filter,amenities,matcher);
    }
  }
  return amenities;
}
