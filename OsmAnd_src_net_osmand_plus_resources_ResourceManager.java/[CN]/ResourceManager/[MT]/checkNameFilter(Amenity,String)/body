{
  boolean publish=false;
  if (filterByName == null || filterByName.length() == 0) {
    publish=true;
  }
 else {
    String lower=OsmAndFormatter.getPoiStringWithoutType(object,context.getSettings().usingEnglishNames()).toLowerCase();
    publish=lower.indexOf(filterByName) != -1;
  }
  return publish;
}
