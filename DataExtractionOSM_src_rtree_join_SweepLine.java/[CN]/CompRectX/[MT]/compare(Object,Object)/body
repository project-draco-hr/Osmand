{
  if (o1 instanceof Rect && o2 instanceof Rect) {
    Rect r1=(Rect)o1;
    Rect r2=(Rect)o2;
    if (r1.getMinX() <= r2.getMinX())     return -1;
 else     if (r1.getMinX() == r2.getMinX())     return 0;
 else     return 1;
  }
 else   throw new ClassCastException("Rect.compareTo : wrong object(s) passed");
}
