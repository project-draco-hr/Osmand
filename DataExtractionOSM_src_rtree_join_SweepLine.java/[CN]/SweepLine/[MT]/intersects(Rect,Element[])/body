{
  if (elmts == null || elmts.length < 1)   return null;
  Element dummy;
  if (elmts[0] instanceof LeafElement)   dummy=new LeafElement(rect,128);
 else   dummy=new NonLeafElement(rect,128);
  Element[] dummyArr=new Element[1];
  dummyArr[0]=dummy;
  return sortedIntersectionTest(dummyArr,elmts);
}
