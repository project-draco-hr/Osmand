{
  int i=first;
  int j=second;
  int l=0;
  ABL[] temp=new ABL[third - first + 1];
  while ((i < second) && (j <= third)) {
    if (arrABL[i].minDist <= arrABL[j].minDist)     temp[l++]=arrABL[i++];
 else     temp[l++]=arrABL[j++];
  }
  if (i >= second)   while (j <= third)   temp[l++]=arrABL[j++];
 else   while (i < second)   temp[l++]=arrABL[i++];
  System.arraycopy(temp,0,arrABL,first,temp.length);
}
