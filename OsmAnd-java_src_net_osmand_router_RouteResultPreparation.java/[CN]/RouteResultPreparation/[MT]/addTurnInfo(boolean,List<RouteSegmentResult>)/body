{
  int next=1;
  for (int i=0; i <= result.size(); i=next) {
    TurnType t=null;
    next=i + 1;
    if (i < result.size()) {
      t=getTurnInfo(result,i,leftside);
      if (t != null && i < result.size() - 1) {
        TurnType jt=justifyUTurn(leftside,result,i,t);
        if (jt != null) {
          t=jt;
          next=i + 2;
        }
      }
      result.get(i).setTurnType(t);
    }
  }
}
