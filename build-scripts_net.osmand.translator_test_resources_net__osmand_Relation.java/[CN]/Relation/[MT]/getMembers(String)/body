{
  if (memberEntities == null) {
    return Collections.emptyList();
  }
  if (role == null) {
    return memberEntities.keySet();
  }
  List<Entity> l=new ArrayList<Entity>();
  for (  Entity m : memberEntities.keySet()) {
    if (role.equals(memberEntities.get(m))) {
      l.add(m);
    }
  }
  return l;
}
