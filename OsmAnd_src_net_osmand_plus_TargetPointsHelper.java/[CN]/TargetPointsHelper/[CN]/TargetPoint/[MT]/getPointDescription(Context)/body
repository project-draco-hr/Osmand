{
  if (!intermediate) {
    return new PointDescription(PointDescription.POINT_TYPE_TARGET,ctx.getString(R.string.destination_point,""),getOnlyName());
  }
 else {
    return new PointDescription(PointDescription.POINT_TYPE_TARGET,(index + 1) + ". " + ctx.getString(R.string.intermediate_point,""),getOnlyName());
  }
}
