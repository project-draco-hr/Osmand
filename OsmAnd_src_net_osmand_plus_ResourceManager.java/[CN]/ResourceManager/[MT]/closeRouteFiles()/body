{
  List<String> map=new ArrayList<String>(routingMapFiles.keySet());
  for (  String m : map) {
    try {
      BinaryMapIndexReader ind=routingMapFiles.remove(m);
      if (ind != null) {
        ind.getRaf().close();
      }
    }
 catch (    IOException e) {
      log.error("Error closing resource " + m,e);
    }
  }
}
