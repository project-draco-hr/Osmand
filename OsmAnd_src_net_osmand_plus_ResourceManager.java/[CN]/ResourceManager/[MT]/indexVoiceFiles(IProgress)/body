{
  File file=context.getAppPath(IndexConstants.VOICE_INDEX_DIR);
  file.mkdirs();
  List<String> warnings=new ArrayList<String>();
  if (file.exists() && file.canRead()) {
    for (    File f : file.listFiles()) {
      if (f.isDirectory()) {
        File conf=new File(f,"_config.p");
        if (!conf.exists()) {
          conf=new File(f,"_ttsconfig.p");
        }
        if (conf.exists()) {
          indexFileNames.put(f.getName(),MessageFormat.format("{0,date,dd.MM.yyyy}",new Date(conf.lastModified())));
        }
      }
    }
  }
  return warnings;
}
