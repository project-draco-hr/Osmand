def records(self):
    'Returns all records in a dbf file.'
    if (not self.numRecords):
        self.__dbfHeader()
    records = []
    f = self.__getFileObj(self.dbf)
    f.seek(self.__dbfHeaderLength())
    for i in range(self.numRecords):
        r = self.__record()
        if r:
            records.append(r)
    return records
