{
  String ext=".osm";
  if (url.endsWith(".osm.bz2")) {
    ext=".osm.bz2";
  }
 else   if (url.endsWith(".osm.pbf")) {
    ext=".osm.pbf";
  }
  File toIndex=null;
  File saveTo=new File(osmDirFiles,regionName + ext);
  if (wget == null || wget.trim().length() == 0) {
    toIndex=internalDownload(url,saveTo);
  }
 else {
    toIndex=wgetDownload(url,saveTo);
  }
  if (toIndex == null) {
    saveTo.delete();
  }
  return toIndex;
}
