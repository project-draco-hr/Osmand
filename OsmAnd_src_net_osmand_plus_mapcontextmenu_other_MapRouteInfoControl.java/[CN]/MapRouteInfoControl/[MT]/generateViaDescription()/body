{
  TargetPointsHelper targets=getTargets();
  String via="";
  List<TargetPoint> points=targets.getIntermediatePointsNavigation();
  if (points.size() == 0) {
    return via;
  }
  for (int i=0; i < points.size(); i++) {
    if (i > 0) {
      via+="\n";
    }
    via+=" " + getRoutePointDescription(points.get(i).point,points.get(i).getOnlyName());
  }
  return via;
}
