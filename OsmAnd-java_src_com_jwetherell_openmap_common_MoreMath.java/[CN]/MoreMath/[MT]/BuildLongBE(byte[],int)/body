{
  return (((long)signedToInt(bytevec[0 + offset]) << 56) | ((long)signedToInt(bytevec[1 + offset]) << 48) | ((long)signedToInt(bytevec[2 + offset]) << 40)| ((long)signedToInt(bytevec[3 + offset]) << 32)| ((long)signedToInt(bytevec[4 + offset]) << 24)| ((long)signedToInt(bytevec[5 + offset]) << 16)| ((long)signedToInt(bytevec[6 + offset]) << 8)| (signedToInt(bytevec[7 + offset])));
}
