{
  if (fileHdr.isWriteThr())   RTree.chdNodes.remove(fileName,nodeIndex);
  if (fileHdr.isWriteThr()) {
    ds.writeInt(totElmt);
    ds.writeLong(prnt);
    ds.writeInt(elmtSz);
    ds.writeInt(elmtTp);
    ds.flush();
    setDirty(true);
  }
 else   setDirty(true);
  totalElements=totElmt;
  parent=prnt;
  elementSize=elmtSz;
  elementType=elmtTp;
}
