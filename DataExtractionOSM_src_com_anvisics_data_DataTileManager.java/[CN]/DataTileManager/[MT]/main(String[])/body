{
  int tileX=3;
  int tileY=3;
  int depth=3;
  for (int i=1; i <= depth; i++) {
    for (int j=0; j <= i; j++) {
      int dx=j == 0 ? 0 : -1;
      for (; dx < 1 || (j < i && dx == 1); dx+=2) {
        print(tileX + dx * j,tileY + i);
        print(tileX + i,tileY - dx * j);
        print(tileX - dx * j,tileY - i);
        print(tileX - i,tileY + dx * j);
      }
    }
  }
}
