{
  if (!warnings.isEmpty()) {
    final StringBuilder b=new StringBuilder();
    boolean f=true;
    for (    String w : warnings) {
      if (f) {
        f=false;
      }
 else {
        b.append('\n');
      }
      b.append(w);
    }
    runOnUiThread(new Runnable(){
      @Override public void run(){
        AccessibleToast.makeText(SettingsBaseActivity.this,b.toString(),Toast.LENGTH_LONG).show();
      }
    }
);
  }
}
