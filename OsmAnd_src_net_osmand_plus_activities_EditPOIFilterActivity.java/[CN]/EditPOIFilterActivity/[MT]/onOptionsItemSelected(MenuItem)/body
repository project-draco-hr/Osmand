{
  if (item.getItemId() == FILTER) {
    filterPOI();
    return true;
  }
 else   if (item.getItemId() == DELETE_FILTER) {
    removePoiFilter();
    return true;
  }
 else   if (item.getItemId() == SAVE_FILTER) {
    savePoiFilter();
    return true;
  }
  return super.onOptionsItemSelected(item);
}
