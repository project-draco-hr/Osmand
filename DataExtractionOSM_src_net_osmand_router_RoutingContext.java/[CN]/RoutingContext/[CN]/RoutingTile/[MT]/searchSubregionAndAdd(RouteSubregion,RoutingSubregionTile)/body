{
  for (int i=0; i < subregions.size(); i++) {
    RoutingSubregionTile ts=subregions.get(i);
    if (ts.subregion == s) {
      if (rt != null) {
        subregions.set(i,rt);
        return rt;
      }
      return ts;
    }
  }
  if (rt != null) {
    subregions.add(rt);
    return rt;
  }
  return null;
}
