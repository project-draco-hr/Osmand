{
  for (  RoutingSubregionTile tl : subregionTiles) {
    if (tl.isLoaded()) {
      if (except == null || except.searchSubregionTile(tl.subregion) < 0) {
        tl.unload();
        unloadedTiles++;
        global.size-=tl.tileStatistics.size;
      }
    }
  }
  subregionTiles.clear();
  tiles.clear();
}
