{
  for (  RoutingTile tl : ctx.tiles.valueCollection()) {
    if (tl.isLoaded()) {
      this.tiles.put(tl.getId(),tl);
      for (      RouteSegment rs : tl.routes.valueCollection()) {
        RouteSegment s=rs;
        while (s != null) {
          s.parentRoute=null;
          s.parentSegmentEnd=0;
          s.distanceFromStart=0;
          s.distanceToEnd=0;
          s=s.next;
        }
      }
    }
  }
}
