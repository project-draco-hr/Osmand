{
  int xloc=x31 >> (31 - zoomToLoadTileWithRoads);
  int yloc=y31 >> (31 - zoomToLoadTileWithRoads);
  int l=(xloc << zoomToLoadTileWithRoads) + yloc;
  RoutingTile tl=tiles.get(l);
  if (tl == null) {
    tl=new RoutingTile(xloc,yloc,zoomToLoadTileWithRoads);
    tiles.put(l,tl);
  }
  return tiles.get(l);
}
