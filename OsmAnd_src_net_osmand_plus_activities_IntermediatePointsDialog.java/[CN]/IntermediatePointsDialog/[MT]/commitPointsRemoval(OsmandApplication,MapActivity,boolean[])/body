{
  int cnt=0;
  for (int i=checkedIntermediates.length - 1; i >= 0; i--) {
    if (!checkedIntermediates[i]) {
      cnt++;
    }
  }
  if (cnt > 0) {
    for (int i=checkedIntermediates.length - 1; i >= 0; i--) {
      if (!checkedIntermediates[i]) {
        cnt--;
        app.getTargetPointsHelper().removeWayPoint(mapActivity,cnt == 0,i);
      }
    }
    if (mapActivity != null) {
      mapActivity.getMapLayers().getContextMenuLayer().setLocation(null,"");
    }
  }
}
