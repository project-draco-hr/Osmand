{
  ContentClass[] swigValues=ContentClass.class.getEnumConstants();
  if (swigValue < swigValues.length && swigValue >= 0 && swigValues[swigValue].swigValue == swigValue)   return swigValues[swigValue];
  for (  ContentClass swigEnum : swigValues)   if (swigEnum.swigValue == swigValue)   return swigEnum;
  throw new IllegalArgumentException("No enum " + ContentClass.class + " with value "+ swigValue);
}
