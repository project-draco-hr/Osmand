{
  super(context,attrs);
  setOnItemClickListener(this);
  TintTypedArray a=TintTypedArray.obtainStyledAttributes(context,attrs,TINT_ATTRS,defStyleAttr,0);
  if (a.hasValue(0)) {
    setBackgroundDrawable(a.getDrawable(0));
  }
  if (a.hasValue(1)) {
    setDivider(a.getDrawable(1));
  }
  a.recycle();
}
