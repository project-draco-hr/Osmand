{
  createMenuItem(menu,UPLOAD_ID,R.string.local_openstreetmap_uploadall,R.drawable.ic_action_gup_light,R.drawable.ic_action_gup_dark,MenuItem.SHOW_AS_ACTION_IF_ROOM | MenuItem.SHOW_AS_ACTION_WITH_TEXT);
