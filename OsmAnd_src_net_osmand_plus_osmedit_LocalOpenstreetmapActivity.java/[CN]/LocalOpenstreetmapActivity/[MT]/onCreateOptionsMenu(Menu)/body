{
  createMenuItem(menu,UPLOAD_ID,R.string.local_openstreetmap_uploadall,R.drawable.ic_action_gup_light,R.drawable.ic_action_gup_dark,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM | MenuItemCompat.SHOW_AS_ACTION_WITH_TEXT);
