{
  buffer.append(NameTableCpp.javaRefToCpp(node.getType()));
  buffer.append(" ");
  for (Iterator<?> it=node.fragments().iterator(); it.hasNext(); ) {
    VariableDeclarationFragment f=(VariableDeclarationFragment)it.next();
    f.accept(this);
    if (it.hasNext()) {
      buffer.append(", ");
    }
  }
  return false;
}
