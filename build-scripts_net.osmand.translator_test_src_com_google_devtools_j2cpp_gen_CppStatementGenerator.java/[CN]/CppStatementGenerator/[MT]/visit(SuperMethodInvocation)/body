{
  IMethodBinding binding=Types.getMethodBinding(node);
  if (Modifier.isStatic(binding.getModifiers())) {
    buffer.append("[[super class] ");
  }
 else {
    buffer.append("[super ");
  }
  buffer.append(NameTable.getName(binding));
  printArguments(binding,node.arguments());
  buffer.append(']');
  return false;
}
