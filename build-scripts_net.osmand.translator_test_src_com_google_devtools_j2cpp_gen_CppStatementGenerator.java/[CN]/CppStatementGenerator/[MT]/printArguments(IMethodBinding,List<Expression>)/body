{
  if (method != null && method.isVarargs()) {
    printVarArgs(method,args);
  }
 else   if (!args.isEmpty()) {
    int nArgs=args.size();
    for (int i=0; i < nArgs; i++) {
      Expression arg=args.get(i);
      printArgument(method,arg,i);
      if (i + 1 < nArgs) {
        buffer.append(' ');
      }
    }
  }
}
