{
  buffer.append("[IOSClass ");
  if (componentType.isInterface()) {
    buffer.append("classWithProtocol:@protocol(");
    buffer.append(NameTable.getFullName(componentType));
    buffer.append(')');
  }
 else {
    buffer.append("classWithClass:[");
    buffer.append(NameTable.getFullName(componentType));
    buffer.append(" class]");
  }
  buffer.append(']');
}
