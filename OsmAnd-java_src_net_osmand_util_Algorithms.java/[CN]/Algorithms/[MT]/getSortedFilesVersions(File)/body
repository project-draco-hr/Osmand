{
  File[] listFiles=dir.listFiles();
  if (listFiles != null) {
    Arrays.sort(listFiles,new Comparator<File>(){
      @Override public int compare(      File o1,      File o2){
        return -simplifyName(o1.getName()).compareTo(simplifyName(o2.getName()));
      }
    }
);
  }
  return listFiles;
}
