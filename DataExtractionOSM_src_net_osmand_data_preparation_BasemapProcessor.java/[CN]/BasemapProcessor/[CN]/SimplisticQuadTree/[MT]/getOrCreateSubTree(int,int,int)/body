{
  if (zm <= zoom) {
    return this;
  }
 else {
    initChildren();
    int nx=(x >> (zm - zoom - 1)) - (this.x << 1);
    int ny=(y >> (zm - zoom - 1)) - (this.y << 1);
    if (nx > 1 || nx < 0 || ny > 1 || ny < 0) {
      return null;
    }
    return children[nx * 2 + ny].getOrCreateSubTree(x,y,zm);
  }
}
