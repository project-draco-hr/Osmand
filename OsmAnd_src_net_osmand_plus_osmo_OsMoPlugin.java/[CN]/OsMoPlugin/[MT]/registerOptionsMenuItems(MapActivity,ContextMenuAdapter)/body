{
  helper.item(R.string.osmo_groups).icons(R.drawable.ic_action_eye_dark,R.drawable.ic_action_eye_light).position(6).listen(new OnContextMenuClick(){
    @Override public boolean onContextMenuClick(    int itemId,    int pos,    boolean isChecked,    DialogInterface dialog){
      Intent intent=new Intent(mapActivity,OsMoGroupsActivity.class);
      mapActivity.startActivity(intent);
      return true;
    }
  }
).reg();
}
