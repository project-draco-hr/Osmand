{
  qa.item("Test (send)").icons(R.drawable.ic_action_grefresh_dark,R.drawable.ic_action_grefresh_light).listen(new OnContextMenuClick(){
    @Override public boolean onContextMenuClick(    ArrayAdapter<?> adapter,    int itemId,    int pos,    boolean isChecked){
      final double lat=view.getLatitude();
      final double lon=view.getLongitude();
      tracker.sendCoordinate(lat,lon);
      return true;
    }
  }
).reg();
}
