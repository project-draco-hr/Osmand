{
  super.registerLayers(activity);
  MapInfoLayer layer=activity.getMapLayers().getMapInfoLayer();
  osmoControl=createOsMoControl(activity,layer.getPaintText(),layer.getPaintSubText());
  layer.getMapInfoControls().registerSideWidget(osmoControl,R.drawable.mon_osmo_conn_big,R.string.osmo_control,"osmo_control",false,18);
  layer.recreateControls();
}
