{
  BinaryInspector in=new BinaryInspector();
  if (args == null || args.length == 0) {
    BinaryInspector.printUsage(null);
    return;
  }
  if ("test".equals(args[0])) {
    in.inspector(new String[]{"-vrouting","-osm=" + System.getProperty("maps.dir") + "/map.obf.osm",System.getProperty("maps.dir") + "/Map.obf"});
  }
 else {
    in.inspector(args);
  }
}
