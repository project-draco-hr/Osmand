{
  BinaryInspector in=new BinaryInspector();
  if (args == null || args.length == 0) {
    BinaryInspector.printUsage(null);
    return;
  }
  if ("test".equals(args[0])) {
    in.inspector(new String[]{"-vaddress","-vcities","-vstreetgroups","-vstreets","-vbuildings","-vintersections","-bbox=4.8486,52.3084,4.8747,52.2970",System.getProperty("maps.dir") + "/Netherlands_noord-holland_europe.obf"});
  }
 else {
    in.inspector(args);
  }
}
