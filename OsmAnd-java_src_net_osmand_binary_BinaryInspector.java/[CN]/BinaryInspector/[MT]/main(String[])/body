{
  BinaryInspector in=new BinaryInspector();
  if (args == null || args.length == 0) {
    BinaryInspector.printUsage(null);
    return;
  }
  if ("test".equals(args[0])) {
    in.inspector(new String[]{"-vaddress","-vcities","-vstreetgroups","-vstreets","-lang=ru",System.getProperty("maps.dir") + "/Ukraine_kiev-city_europe.obf"});
  }
 else {
    in.inspector(args);
  }
}
