{
  try {
    if (rtree == null)     throw new IllegalArgumentException("PackTree.packTree: rtree null");
    List elmts=rtree.getAllElements();
    int ret=packTree((Element[])elmts.toArray(new Element[elmts.size()]),rtree,newFile);
    return ret;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return 2;
  }
}
