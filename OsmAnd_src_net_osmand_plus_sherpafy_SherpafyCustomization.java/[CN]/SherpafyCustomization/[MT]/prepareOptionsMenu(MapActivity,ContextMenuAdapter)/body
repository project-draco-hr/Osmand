{
  filter(adapter,R.string.exit_Button,R.string.menu_layers,R.string.pause_navigation,R.string.continue_navigation,R.string.cancel_navigation,R.string.cancel_route,R.string.clear_destination,R.string.get_directions,R.string.menu_mute_on,R.string.menu_mute_off,R.string.where_am_i);
  adapter.item(R.string.sherpafy_tour_info_txt).icons(R.drawable.ic_action_info_dark,R.drawable.ic_action_info_light).position(4).listen(new OnContextMenuClick(){
    @Override public void onContextMenuClick(    int itemId,    int pos,    boolean isChecked,    DialogInterface dialog){
      Intent newIntent=new Intent(mapActivity,TourViewActivity.class);
      mapActivity.startActivity(newIntent);
    }
  }
).reg();
}
