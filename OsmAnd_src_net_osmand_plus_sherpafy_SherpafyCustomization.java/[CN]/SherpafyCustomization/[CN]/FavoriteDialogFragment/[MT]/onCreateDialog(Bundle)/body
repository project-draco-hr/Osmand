{
  Bundle args=getArguments();
  SherpafyFavoriteFragment ssf=new SherpafyFavoriteFragment();
  ssf.setArguments(args);
  ssf.onAttach(getActivity());
  AlertDialog dlg=new AlertDialog.Builder(getActivity()).setView(ssf.onCreateView(getActivity().getLayoutInflater(),null,savedInstanceState)).setPositiveButton(R.string.default_buttons_ok,null).create();
  return dlg;
}
