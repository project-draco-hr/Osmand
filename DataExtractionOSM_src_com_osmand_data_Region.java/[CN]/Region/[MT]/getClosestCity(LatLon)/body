{
  City closest=null;
  double relDist=Double.POSITIVE_INFINITY;
  for (  City c : cityManager.getClosestObjects(point.getLatitude(),point.getLongitude())) {
    double rel=MapUtils.getDistance(c.getEntity(),point) / c.getType().getRadius();
    if (rel < relDist) {
      closest=c;
      relDist=rel;
      if (relDist < 0.2d) {
        break;
      }
    }
  }
  return closest;
}
