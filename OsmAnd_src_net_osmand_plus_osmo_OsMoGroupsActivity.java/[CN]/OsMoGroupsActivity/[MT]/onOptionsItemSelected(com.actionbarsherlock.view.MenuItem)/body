{
  if (item.getItemId() == CONNECT_TO_DEVICE) {
    connectToDevice();
    return true;
  }
 else   if (item.getItemId() == JOIN_GROUP) {
    return true;
  }
 else   if (item.getItemId() == DELETE_ID) {
    return true;
  }
 else   if (item.getItemId() == CREATE_GROUP) {
    return true;
  }
 else {
    return super.onOptionsItemSelected(item);
  }
}
