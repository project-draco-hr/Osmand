{
  Builder bld=new AlertDialog.Builder(this);
  bld.setItems(new String[]{getString(R.string.osmo_share_current_session),getString(R.string.osmo_share_connect_device)},new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int which){
      if (which == 0) {
        shareSessionUrl();
      }
 else {
        OsMoService service=osMoPlugin.getService();
        SessionInfo ci=service.getCurrentSessionInfo();
        shareTrackerId("",ci == null ? null : ci.trackerId);
      }
    }
  }
);
  bld.setNegativeButton(R.string.default_buttons_cancel,null);
  bld.show();
}
