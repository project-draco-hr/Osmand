{
  if (p.getName().equals("") && flatGroups.containsKey(p.getCategory())) {
    return true;
  }
  FavoriteGroup group=getOrCreateGroup(p,0);
  checkDublicates(p);
  if (!p.getName().equals("")) {
    p.setVisible(group.visible);
    p.setColor(group.color);
    group.points.add(p);
    cachedFavoritePoints.add(p);
  }
  if (saveImmediately) {
    saveCurrentPointsIntoFile();
  }
  return true;
}
