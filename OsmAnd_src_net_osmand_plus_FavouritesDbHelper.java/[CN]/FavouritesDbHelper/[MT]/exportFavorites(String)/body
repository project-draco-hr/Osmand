{
  File f=new File(OsmandApplication.getSettings().extendOsmandPath(ResourceManager.APP_DIR),fileName);
  GPXFile gpx=new GPXFile();
  for (  FavouritePoint p : getFavouritePoints()) {
    if (p.isStored()) {
      WptPt pt=new WptPt();
      pt.lat=p.getLatitude();
      pt.lon=p.getLongitude();
      if (p.getCategory().length() > 0)       pt.name=p.getName() + "_" + p.getCategory();
 else       pt.name=p.getName();
      gpx.points.add(pt);
    }
  }
  return GPXUtilities.writeGpxFile(f,gpx,context);
}
