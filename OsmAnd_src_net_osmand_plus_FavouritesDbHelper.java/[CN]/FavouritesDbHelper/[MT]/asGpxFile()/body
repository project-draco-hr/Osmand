{
  GPXFile gpx=new GPXFile();
  for (  FavouritePoint p : getFavouritePoints()) {
    WptPt pt=new WptPt();
    pt.lat=p.getLatitude();
    pt.lon=p.getLongitude();
    pt.name=p.getName();
    if (p.getCategory().length() > 0)     pt.category=p.getCategory();
    gpx.points.add(pt);
  }
  return gpx;
}
