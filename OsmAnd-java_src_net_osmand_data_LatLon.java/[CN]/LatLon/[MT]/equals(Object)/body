{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  LatLon other=(LatLon)obj;
  if (Float.floatToIntBits((float)latitude) != Float.floatToIntBits((float)other.latitude))   return false;
  if (Float.floatToIntBits((float)longitude) != Float.floatToIntBits((float)other.longitude))   return false;
  return true;
}
