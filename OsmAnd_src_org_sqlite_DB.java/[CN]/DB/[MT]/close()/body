{
synchronized (stmts) {
    Iterator i=stmts.entrySet().iterator();
    while (i.hasNext()) {
      Map.Entry entry=(Map.Entry)i.next();
      Stmt stmt=(Stmt)entry.getValue();
      finalize(((Long)entry.getKey()).longValue());
      if (stmt != null) {
        stmt.pointer=0;
      }
      i.remove();
    }
  }
  free_functions();
  if (begin != 0) {
    finalize(begin);
    begin=0;
  }
  if (commit != 0) {
    finalize(commit);
    commit=0;
  }
  _close();
}
