{
  if (getCurrentRunningTask() instanceof ReloadIndexesTask) {
    if (checkRunning()) {
      return;
    }
  }
  for (  IndexItem i : items) {
    indexItemDownloading.add(i);
  }
  if (currentDownloadingItem == null) {
    execute(new DownloadIndexesAsyncTask());
  }
 else {
    downloadInProgress();
  }
}
