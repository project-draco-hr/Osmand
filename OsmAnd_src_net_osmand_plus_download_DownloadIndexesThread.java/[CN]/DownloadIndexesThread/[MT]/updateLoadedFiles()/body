{
  Map<String,String> indexActivatedFileNames=app.getResourceManager().getIndexFileNames();
  DownloadIndexActivity.listWithAlternatives(app.getAppPath(""),IndexConstants.EXTRA_EXT,indexActivatedFileNames);
  Map<String,String> indexFileNames=app.getResourceManager().getIndexFileNames();
  DownloadIndexActivity.listWithAlternatives(app.getAppPath(""),IndexConstants.EXTRA_EXT,indexFileNames);
  DownloadIndexActivity.listWithAlternatives(app.getAppPath(IndexConstants.TILES_INDEX_DIR),IndexConstants.SQLITE_EXT,indexFileNames);
  app.getResourceManager().getBackupIndexes(indexFileNames);
  this.indexFileNames=indexFileNames;
  this.indexActivatedFileNames=indexActivatedFileNames;
}
