{
  boolean exceed=Version.isFreeVersion(app) && DownloadActivityType.isCountedInDownloads(item) && downloads.get() >= DownloadActivity.MAXIMUM_AVAILABLE_FREE_DOWNLOADS;
  if (exceed) {
    String breakDownloadMessage=app.getString(R.string.free_version_message,DownloadActivity.MAXIMUM_AVAILABLE_FREE_DOWNLOADS + "");
    publishProgress(breakDownloadMessage);
  }
  return !exceed;
}
