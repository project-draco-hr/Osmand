{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  LocationPointWrapper other=(LocationPointWrapper)obj;
  if (point == null) {
    if (other.point != null) {
      return false;
    }
  }
 else   if (!point.equals(other.point)) {
    return false;
  }
  return true;
}
