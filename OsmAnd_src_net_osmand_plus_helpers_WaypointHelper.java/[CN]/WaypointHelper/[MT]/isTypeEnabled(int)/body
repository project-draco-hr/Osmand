{
  if (type == ALARMS) {
    return showAlarms() || announceAlarms();
  }
 else   if (type == POI) {
    return showPOI() || announcePOI();
  }
 else   if (type == FAVORITES) {
    return showFavorites() || announceFavorites();
  }
 else   if (type == WAYPOINTS) {
    return showGPXWaypoints() || announceGPXWaypoints();
  }
  return true;
}
