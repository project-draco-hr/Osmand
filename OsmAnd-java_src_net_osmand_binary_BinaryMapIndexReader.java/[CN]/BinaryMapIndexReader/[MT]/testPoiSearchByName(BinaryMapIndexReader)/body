{
  println("Searching by name...");
  int tileZ=31 - 14;
  SearchRequest<Amenity> req=buildSearchPoiRequest(sleft / 2 + sright / 2,stop / 2 + sbottom / 2,"kol",sleft - 1 << tileZ,sleft + 1 << tileZ,stop - 1 << tileZ,stop + 1 << tileZ,null);
  reader.searchPoiByName(req);
  for (  Amenity a : req.getSearchResults()) {
    println(a.getType() + " " + a.getSubType()+ " "+ a.getName()+ " "+ a.getLocation());
  }
}
