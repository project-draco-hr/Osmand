{
  try {
    log.debug("Start loading list of index files");
    Map<String,String> indexFiles=DownloaderIndexFromGoogleCode.getIndexFiles(new String[]{IndexConstants.ADDRESS_INDEX_EXT,IndexConstants.POI_INDEX_EXT},new String[]{IndexConstants.ADDRESS_TABLE_VERSION + "",IndexConstants.POI_TABLE_VERSION + ""});
    if (indexFiles != null && !indexFiles.isEmpty()) {
      return indexFiles;
    }
 else {
      return null;
    }
  }
 catch (  RuntimeException e) {
    log.error("Error while loading indexes from repository",e);
    return null;
  }
}
