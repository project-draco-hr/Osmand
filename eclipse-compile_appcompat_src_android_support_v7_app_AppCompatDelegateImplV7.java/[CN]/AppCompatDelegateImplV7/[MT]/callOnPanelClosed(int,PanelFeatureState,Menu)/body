{
  if (menu == null) {
    if (panel == null) {
      if ((featureId >= 0) && (featureId < mPanels.length)) {
        panel=mPanels[featureId];
      }
    }
    if (panel != null) {
      menu=panel.menu;
    }
  }
  if ((panel != null) && (!panel.isOpen))   return;
  Window.Callback cb=getWindowCallback();
  if (cb != null) {
    cb.onPanelClosed(featureId,menu);
  }
}
