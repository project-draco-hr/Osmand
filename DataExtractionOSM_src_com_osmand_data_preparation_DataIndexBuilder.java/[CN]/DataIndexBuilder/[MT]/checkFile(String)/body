{
  if (zipped && !name.endsWith(".bz2")) {
    name+=".bz2";
  }
  File f=new File(workingDir,name);
  f.mkdirs();
  if (f.exists()) {
    f.delete();
  }
  OutputStream output=new FileOutputStream(f);
  if (name.endsWith(".bz2")) {
    output.write('B');
    output.write('Z');
    output=new CBZip2OutputStream(output);
  }
  return output;
}
