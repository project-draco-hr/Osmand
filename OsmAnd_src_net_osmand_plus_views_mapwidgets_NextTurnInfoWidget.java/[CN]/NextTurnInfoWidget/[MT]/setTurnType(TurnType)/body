{
  updateVisibility(turnType != null);
  if (turnDrawable.setTurnType(turnType)) {
    if (horisontalMini) {
      setImageDrawable(turnDrawable,false);
    }
 else {
      setTopImageDrawable(turnDrawable,turnType == null || turnType.getExitOut() == 0 ? "" : turnType.getExitOut() + "");
    }
  }
}
