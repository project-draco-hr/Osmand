{
  if (members != null) {
    if (memberEntities == null) {
      memberEntities=new LinkedHashMap<Entity,String>();
    }
 else {
      memberEntities.clear();
    }
    for (    EntityId l : members.keySet()) {
      if (l != null && entities.get(l) != null) {
        memberEntities.put(entities.get(l),members.get(l));
      }
    }
  }
}
