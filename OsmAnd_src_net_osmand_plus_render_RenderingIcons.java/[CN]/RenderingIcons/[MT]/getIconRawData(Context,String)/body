{
  Integer resId=icons.get(s);
  if (resId == null)   return null;
  try {
    Bitmap ic=getIcon(ctx,s);
    if (ic == null) {
      return null;
    }
    ByteBuffer buf=ByteBuffer.allocate(ic.getByteCount());
    ic.copyPixelsToBuffer(buf);
    return buf.array();
  }
 catch (  Throwable e) {
    log.error("Failed to get byte stream from icon",e);
    return null;
  }
}
