{
  settingsAPI.edit(globalPreferences).putFloat(POINT_NAVIGATE_LAT_BACKUP,settingsAPI.getFloat(globalPreferences,POINT_NAVIGATE_LAT,0)).putFloat(POINT_NAVIGATE_LON_BACKUP,settingsAPI.getFloat(globalPreferences,POINT_NAVIGATE_LON,0)).putString(POINT_NAVIGATE_DESCRIPTION_BACKUP,settingsAPI.getString(globalPreferences,POINT_NAVIGATE_DESCRIPTION,"")).commit();
}
