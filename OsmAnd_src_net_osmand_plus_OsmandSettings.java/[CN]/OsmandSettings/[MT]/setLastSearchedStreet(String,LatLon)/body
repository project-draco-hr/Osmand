{
  Editor edit=globalPreferences.edit().putString(LAST_SEARCHED_STREET,street).putString(LAST_SEARCHED_BUILDING,"");
  if (globalPreferences.contains(LAST_SEARCHED_INTERSECTED_STREET)) {
    edit.putString(LAST_SEARCHED_INTERSECTED_STREET,"");
  }
  boolean res=edit.commit();
  setLastSearchedPoint(point);
  return res;
}
