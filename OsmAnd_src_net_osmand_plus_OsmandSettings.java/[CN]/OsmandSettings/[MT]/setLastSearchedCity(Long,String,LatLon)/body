{
  Editor edit=globalPreferences.edit().putLong(LAST_SEARCHED_CITY,cityId).putString(LAST_SEARCHED_CITY_NAME,name).putString(LAST_SEARCHED_STREET,"").putString(LAST_SEARCHED_BUILDING,"");
  edit.remove(lAST_SEARCHED_POSTCODE);
  if (globalPreferences.contains(LAST_SEARCHED_INTERSECTED_STREET)) {
    edit.putString(LAST_SEARCHED_INTERSECTED_STREET,"");
  }
  boolean res=edit.commit();
  setLastSearchedPoint(point);
  return res;
}
