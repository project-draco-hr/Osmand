{
  final String value=System.getenv("SECONDARY_STORAGE");
  if (!TextUtils.isEmpty(value)) {
    final String[] paths=value.split(":");
    for (    String path : paths) {
      File file=new File(path);
      if (file.isDirectory() && file.canWrite()) {
        return file;
      }
    }
  }
  return null;
}
