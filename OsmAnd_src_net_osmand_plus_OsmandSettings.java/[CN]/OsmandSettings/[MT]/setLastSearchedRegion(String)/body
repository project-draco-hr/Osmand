{
  Editor edit=globalPreferences.edit().putString(LAST_SEARCHED_REGION,region).putLong(LAST_SEARCHED_CITY,-1).putString(LAST_SEARCHED_STREET,"").putString(LAST_SEARCHED_BUILDING,"");
  if (globalPreferences.contains(LAST_SEARCHED_INTERSECTED_STREET)) {
    edit.putString(LAST_SEARCHED_INTERSECTED_STREET,"");
  }
  return edit.commit();
}
