{
  File tPath=extendOsmandPath(ResourceManager.TILES_PATH);
  File dir=new File(tPath,toInstall.getName());
  dir.mkdirs();
  if (dir.exists() && dir.isDirectory()) {
    try {
      TileSourceManager.createMetaInfoFile(dir,toInstall,true);
    }
 catch (    IOException e) {
      return false;
    }
  }
  return true;
}
