{
  Set<String> set=getEnabledPlugins();
  if (enable) {
    set.add(pluginId);
  }
 else {
    set.remove(pluginId);
  }
  StringBuilder serialization=new StringBuilder();
  Iterator<String> it=set.iterator();
  while (it.hasNext()) {
    serialization.append(it.next());
    if (it.hasNext()) {
      serialization.append(",");
    }
  }
  ENABLED_PLUGINS.set(serialization.toString());
}
