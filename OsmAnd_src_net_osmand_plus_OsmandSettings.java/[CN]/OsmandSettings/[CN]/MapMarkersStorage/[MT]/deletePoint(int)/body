{
  List<LatLon> ps=getPoints();
  List<String> ds=getPointDescriptions(ps.size());
  List<Integer> cs=getColors(ps.size());
  List<Integer> ns=getPositions(ps.size());
  ps.remove(index);
  ds.remove(index);
  if (cs.size() > index) {
    cs.remove(index);
  }
  if (ns.size() > index) {
    ns.remove(index);
  }
  return savePoints(ps,ds,cs,ns);
}
