{
  new AsyncTask<String,Void,List<SearchItem>>(){
    @Override protected List<SearchItem> doInBackground(    String... params){
      return doSearch(params[0]);
    }
    @Override protected void onPostExecute(    List<SearchItem> searchItems){
      if (onFinished != null) {
        onFinished.run();
      }
      if (apiCallback != null) {
        apiCallback.onSearchFinished(searchItems,cancelled);
      }
    }
  }
.execute(keyword);
}
