{
  name=parser.isNamespaceAware() ? localName : name;
  if ("filter".equals(name)) {
    stack.pop();
  }
 else   if ("group".equals(name)) {
    GroupRules group=(GroupRules)stack.pop();
    if (stack.size() == 0) {
      group.registerGlobalRules(state);
    }
  }
 else   if ("groupFilter".equals(name)) {
    stack.pop();
  }
}
