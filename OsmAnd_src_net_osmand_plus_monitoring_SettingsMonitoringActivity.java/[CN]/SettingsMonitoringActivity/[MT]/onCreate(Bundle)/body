{
  requestWindowFeature(Window.FEATURE_INDETERMINATE_PROGRESS);
  super.onCreate(savedInstanceState);
  setSupportProgressBarIndeterminateVisibility(false);
  getSupportActionBar().setTitle(R.string.monitoring_settings);
