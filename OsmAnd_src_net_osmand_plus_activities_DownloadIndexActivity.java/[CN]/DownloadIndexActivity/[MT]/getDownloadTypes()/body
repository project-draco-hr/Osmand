{
  DownloadActivityType[] items;
  if (OsmandPlugin.getEnabledPlugin(SRTMPlugin.class) != null) {
    items=new DownloadActivityType[]{DownloadActivityType.NORMAL_FILE,DownloadActivityType.ROADS_FILE,DownloadActivityType.SRTM_FILE,DownloadActivityType.HILLSHADE_FILE};
  }
 else {
    items=new DownloadActivityType[]{DownloadActivityType.NORMAL_FILE,DownloadActivityType.ROADS_FILE};
  }
  return items;
}
