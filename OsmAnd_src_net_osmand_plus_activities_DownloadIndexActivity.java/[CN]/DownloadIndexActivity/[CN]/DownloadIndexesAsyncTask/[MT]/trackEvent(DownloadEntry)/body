{
  String v=Version.getAppName(getMyApplication());
  if (Version.isProductionVersion(getMyApplication())) {
    v=Version.getFullVersion(getMyApplication());
  }
 else {
    v+=" test";
  }
  new DownloadTracker().trackEvent(DownloadIndexActivity.this,v,Version.getAppName(getMyApplication()),entry.baseName,1,DownloadIndexActivity.this.getString(R.string.ga_api_key));
}
