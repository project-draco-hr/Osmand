{
  List<Way> result=new ArrayList<Way>(inners.size());
  for (  Way in : inners) {
    boolean inIsIn=true;
    for (    Node n : in.getNodes()) {
      if (!polygonContainsPoint(n.getLatitude(),n.getLongitude(),outer,null)) {
        inIsIn=false;
        break;
      }
    }
    if (inIsIn) {
      result.add(in);
    }
  }
  return result;
}
