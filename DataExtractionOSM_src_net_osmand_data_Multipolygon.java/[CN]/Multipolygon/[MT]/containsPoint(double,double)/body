{
  boolean outerContain=false;
  for (  Ring outer : getOuterRings()) {
    if (outer.containsPoint(latitude,longitude)) {
      outerContain=true;
      break;
    }
  }
  if (!outerContain) {
    return false;
  }
  for (  Ring inner : getInnerRings()) {
    if (inner.containsPoint(latitude,longitude)) {
      return false;
    }
  }
  return true;
}
