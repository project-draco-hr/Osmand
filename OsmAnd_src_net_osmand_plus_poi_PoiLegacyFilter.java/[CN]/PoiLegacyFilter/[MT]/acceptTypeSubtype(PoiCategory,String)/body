{
  if (t == null) {
    return true;
  }
  if (!poiTypes.isRegisteredType(t)) {
    t=poiTypes.getOtherPoiCategory();
  }
  if (!acceptedTypes.containsKey(t)) {
    return false;
  }
  LinkedHashSet<String> set=acceptedTypes.get(t);
  if (set == null) {
    return true;
  }
  return set.contains(subtype);
}
