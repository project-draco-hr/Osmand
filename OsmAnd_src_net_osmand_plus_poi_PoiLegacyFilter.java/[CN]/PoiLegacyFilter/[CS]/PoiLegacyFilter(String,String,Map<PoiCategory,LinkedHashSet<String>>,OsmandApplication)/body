{
  this.app=app;
  isStandardFilter=false;
  poiTypes=app.getPoiTypes();
  if (filterId == null) {
    filterId=USER_PREFIX + name.replace(' ','_').toLowerCase();
  }
  this.filterId=filterId;
  this.name=name;
  if (acceptedTypes == null) {
    initSearchAll();
  }
 else {
    this.acceptedTypes.putAll(acceptedTypes);
  }
}
