{
  checkOsmbugsPoints();
  SQLiteDatabase db=getWritableDatabase();
  if (db != null) {
    db.execSQL("DELETE FROM " + OSMBUGS_TABLE_NAME + " WHERE "+ OSMBUGS_COL_ID+ " = ? AND "+ OSMBUGS_COL_TEXT+ " = ? AND "+ OSMBUGS_COL_LAT+ " = ? AND "+ OSMBUGS_COL_LON+ " = ? AND "+ OSMBUGS_COL_ACTION+ " = ? AND "+ OSMBUGS_COL_AUTHOR+ " = ?",new Object[]{p.getId(),p.getText(),p.getLatitude(),p.getLongitude(),OsmBugsRemoteUtil.stringAction.get(p.getAction()),p.getAuthor()});
    cachedOsmbugsPoints.remove(p);
    p.setStored(false);
    return true;
  }
  return false;
}
