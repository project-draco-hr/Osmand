{
  if (!currentTrack && this.zoom != zoom) {
    if (culler != null) {
      culler.cancel(true);
    }
    double cullDistance=Math.pow(2.0,base - zoom);
    culler=new AsynchronousResampler.RamerDouglasPeucer(this,cullDistance);
    if (this.zoom < zoom) {
      culled=null;
    }
    this.zoom=zoom;
    culler.execute("");
  }
}
