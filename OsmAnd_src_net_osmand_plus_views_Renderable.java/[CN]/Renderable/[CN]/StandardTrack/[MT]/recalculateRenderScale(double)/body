{
  double hashCode=points.hashCode();
  if (hashPoint != hashCode) {
    if (culler != null) {
      culler.cancel(true);
      culled=null;
    }
  }
 else   if (culler == null || hashZoom != zoom) {
    if (culler != null) {
      culler.cancel(true);
    }
    double cullDistance=Math.pow(2.0,base - zoom);
    culler=new AsynchronousResampler.RamerDouglasPeucer(this,cullDistance);
    if (hashZoom < zoom) {
      culled=null;
    }
    hashZoom=zoom;
    culler.execute("");
  }
}
