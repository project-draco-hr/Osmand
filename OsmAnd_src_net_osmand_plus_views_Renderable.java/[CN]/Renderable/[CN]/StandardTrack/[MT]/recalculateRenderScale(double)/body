{
  if (!currentTrack && zoom != newZoom) {
    if (culler != null) {
      culler.cancel(true);
    }
    double cullDistance=Math.pow(2.0,base - zoom);
    culler=new AsynchronousResampler.RamerDouglasPeucer(this,cullDistance);
    if (zoom < newZoom) {
      culled=null;
    }
    zoom=newZoom;
    culler.execute("");
  }
}
