{
  if (!hasName)   return false;
  if (hasCities()) {
    if (!getCities().isInitialized())     return false;
  }
  if (hasPostcodes()) {
    if (!getPostcodes().isInitialized())     return false;
  }
  if (hasVillages()) {
    if (!getVillages().isInitialized())     return false;
  }
  return true;
}
