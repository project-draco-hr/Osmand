{
  threadPoolExecutor=new ThreadPoolExecutor(numberOfThreads,numberOfThreads,TILE_DOWNLOAD_SECONDS_TO_WORK,TimeUnit.SECONDS,new LIFOBlockingDeque<Runnable>());
  pendingToDownload=Collections.synchronizedSet(new HashSet<File>());
