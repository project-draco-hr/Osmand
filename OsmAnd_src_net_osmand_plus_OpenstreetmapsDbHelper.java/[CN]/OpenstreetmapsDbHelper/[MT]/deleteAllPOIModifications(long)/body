{
  checkOpenstreetmapPoints();
  SQLiteDatabase db=getWritableDatabase();
  if (db != null) {
    db.execSQL("DELETE FROM " + OPENSTREETMAP_TABLE_NAME + " WHERE "+ OPENSTREETMAP_COL_ID+ " = ?",new Object[]{id});
    for (Iterator<OpenstreetmapPoint> it=cachedOpenstreetmapPoints.iterator(); it.hasNext(); ) {
      if (it.next().getId() == id) {
        it.remove();
      }
    }
    ;
    return true;
  }
  return false;
}
