{
  int count=0;
  int total=0;
  for (  GpxInfo info : params) {
    if (!isCancelled() && info.file != null) {
      String warning=null;
      File file=info.file;
      warning=new OpenstreetmapRemoteUtil(la).uploadGPXFile(tagstring,description,visibility,file);
      total++;
      if (warning == null) {
        count++;
      }
 else {
        publishProgress(warning);
      }
    }
  }
  return la.getString(R.string.local_index_items_uploaded,count,total);
}
