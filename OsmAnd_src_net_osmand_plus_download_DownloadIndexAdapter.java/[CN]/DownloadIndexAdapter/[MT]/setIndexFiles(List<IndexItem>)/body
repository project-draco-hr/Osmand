{
  this.indexFiles.clear();
  for (  IndexItem i : indexFiles) {
    this.indexFiles.add(i);
  }
  List<IndexItemCategory> cats=IndexItemCategory.categorizeIndexItems(downloadActivity.getClientContext(),indexFiles);
synchronized (this) {
    list.clear();
    list.addAll(cats);
  }
  notifyDataSetChanged();
}
