{
  OsmandSettings settings=getMyApplication().getSettings();
  indexActivatedFileNames=getMyApplication().getResourceManager().getIndexFileNames();
  DownloadIndexActivity.listWithAlternatives(settings.extendOsmandPath(ResourceManager.APP_DIR),EXTRA_EXT,indexActivatedFileNames);
  indexFileNames=getMyApplication().getResourceManager().getIndexFileNames();
  DownloadIndexActivity.listWithAlternatives(settings.extendOsmandPath(ResourceManager.APP_DIR),EXTRA_EXT,indexFileNames);
  getMyApplication().getResourceManager().getBackupIndexes(indexFileNames);
}
