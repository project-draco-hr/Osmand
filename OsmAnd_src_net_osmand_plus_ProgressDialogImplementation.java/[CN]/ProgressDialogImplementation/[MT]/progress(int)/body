{
  this.progress+=deltaWork;
  final int prg=progress;
  if (!isIndeterminate() && dialog != null) {
    if (activity != null) {
      activity.runOnUiThread(new Runnable(){
        @Override public void run(){
          dialog.setProgress(prg);
        }
      }
);
    }
 else {
      dialog.setProgress(prg);
    }
  }
}
