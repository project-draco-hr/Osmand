{
  checkOpen();
  if (rs.isOpen())   throw new SQLException("ResultSet already requested");
  if (db.column_count(pointer) == 0)   throw new SQLException("no ResultSet available");
  if (rs.colsMeta == null)   rs.colsMeta=db.column_names(pointer);
  rs.cols=rs.colsMeta;
  rs.open=resultsWaiting;
  resultsWaiting=false;
  return rs;
}
