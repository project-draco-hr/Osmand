{
  this.view=view;
  pixRect=new Rect();
  tileRect=new RectF();
  paintInt=new Paint();
  paintInt.setColor(Color.rgb(50,200,50));
  paintInt.setAlpha(150);
  paintInt.setAntiAlias(true);
  paintEnd=new Paint();
  paintEnd.setColor(Color.rgb(255,0,0));
  paintEnd.setAlpha(150);
  paintEnd.setAntiAlias(true);
}
