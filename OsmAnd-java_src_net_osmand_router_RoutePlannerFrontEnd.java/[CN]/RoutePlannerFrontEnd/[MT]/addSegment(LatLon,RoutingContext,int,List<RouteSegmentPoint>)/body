{
  RouteSegmentPoint f=findRouteSegment(s.getLatitude(),s.getLongitude(),false,ctx);
  if (f == null) {
    ctx.calculationProgress.segmentNotFound=indexNotFound;
    return false;
  }
 else {
    log.info("Route segment found " + f.getRoad().id + " "+ f.getRoad().getName());
    res.add(f);
    return true;
  }
}
