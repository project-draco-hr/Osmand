{
  new AsyncTask<Void,Void,List<SearchObject>>(){
    @Override protected List<SearchObject> doInBackground(    Void... params){
      return doSearch();
    }
    @Override protected void onPostExecute(    List<SearchObject> searchObjects){
      onSearchRequestPostExecute(searchObjects);
      if (onFinished != null) {
        onFinished.run();
      }
      if (searchCallback != null && !cancelled) {
        searchCallback.onSearchFinished(searchObjects);
      }
    }
  }
.execute();
}
