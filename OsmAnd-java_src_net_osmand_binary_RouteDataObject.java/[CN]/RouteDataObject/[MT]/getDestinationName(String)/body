{
  if (names != null) {
    if (Algorithms.isEmpty(lang)) {
      return names.get(region.destinationTypeRule);
    }
    int[] kt=names.keys();
    for (int i=0; i < kt.length; i++) {
      int k=kt[i];
      if (region.routeEncodingRules.size() > k) {
        if (("destination:" + lang).equals(region.routeEncodingRules.get(k).getTag())) {
          return names.get(k);
        }
      }
    }
    return names.get(region.destinationTypeRule);
  }
  return null;
}
