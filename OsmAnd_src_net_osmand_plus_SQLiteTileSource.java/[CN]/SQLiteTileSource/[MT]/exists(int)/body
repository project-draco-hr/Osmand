{
  SQLiteDatabase db=getDatabase();
  if (db == null) {
    return false;
  }
  Cursor cursor=db.rawQuery("SELECT 1 FROM tiles WHERE z = ?",new String[]{(17 - zoom) + ""});
  try {
    boolean e=cursor.moveToFirst();
    cursor.close();
    return e;
  }
 catch (  SQLiteDiskIOException e) {
    return false;
  }
}
