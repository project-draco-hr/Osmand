{
  if ((db == null || db.isClosed()) && file.exists()) {
    db=ctx.getSQLiteAPI().openByAbsolutePath(file.getAbsolutePath(),false);
    try {
      String template=db.compileStatement("SELECT url FROM info").simpleQueryForString();
      if (!Algorithms.isEmpty(template)) {
        urlTemplate=template;
      }
    }
 catch (    RuntimeException e) {
    }
    try {
      long z;
      z=db.compileStatement("SELECT minzoom FROM info").simpleQueryForLong();
      if (z < 17)       baseZoom=17 - (int)z;
      maxZoom=24;
      while ((tileSize >> (maxZoom - baseZoom)) < minScaledSize)       maxZoom--;
      z=db.compileStatement("SELECT maxzoom FROM info").simpleQueryForLong();
      if (z < 17)       minZoom=17 - (int)z;
    }
 catch (    RuntimeException e) {
    }
  }
  return db;
}
