{
  this.ctx=ctx;
  this.file=f;
  int i=f.getName().lastIndexOf('.');
  name=f.getName().substring(0,i);
  i=name.lastIndexOf('.');
  if (i > 0) {
    String sourceName=name.substring(i + 1);
    for (    TileSourceTemplate is : toFindUrl) {
      if (is.getName().equalsIgnoreCase(sourceName)) {
        base=is;
        urlTemplate=is.getUrlTemplate();
        break;
      }
    }
  }
}
