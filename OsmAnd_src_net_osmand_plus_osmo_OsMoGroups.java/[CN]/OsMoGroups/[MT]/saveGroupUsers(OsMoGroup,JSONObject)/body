{
  JSONArray ar=new JSONArray();
  for (  OsMoUser u : gr.users.values()) {
    JSONObject obj=new JSONObject();
    if (u.userName != null) {
      obj.put(USER_NAME,u.userName);
    }
    if (u.serverName != null) {
      obj.put("serverName",u.serverName);
    }
    obj.put(TRACKER_ID,u.trackerId);
    ar.put(obj);
  }
  grObj.put("users",ar);
}
