{
  if (type == VIEW) {
    return ctx.getString(R.string.show_details);
  }
 else   if (type == EMAIL) {
    return "Email";
  }
 else   if (type == SMS) {
    return "SMS";
  }
 else   if (type == CLIPBOARD) {
    return "Clipboard";
  }
 else   if (type == QR) {
    return "QR-code";
  }
  return "";
}
