{
  this.distance=distance;
  this.time=time;
  if (distance > 1500) {
    return 0;
  }
  if ((time > 0 && time < 12) || distance < 150 || type == SPEED_LIMIT) {
    return type;
  }
  if (type == SPEED_CAMERA && (time < 20 || distance < 250)) {
    return type;
  }
  if ((time > 0 && time < 18) || distance < 300) {
    return type + MAXIMUM;
  }
  return 0;
}
