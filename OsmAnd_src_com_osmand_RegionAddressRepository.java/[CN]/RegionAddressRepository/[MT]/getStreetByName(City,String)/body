{
  if (city.isEmptyWithStreets()) {
    preloadStreets(city);
  }
  if (postCodes.isEmpty()) {
    preloadPostcodes();
  }
  Street street=city.getStreet(name);
  if (street == null) {
    street=postCodes.contains(name.toUpperCase()) ? new PostcodeBasedStreet(city,name) : null;
  }
  return street;
}
