{
  int val=(int)lval;
  if (topIdx >= (stkLimit - 1))   throw new StackOverflowException("FileHdr.push: Overflow but not fatal");
  S[++topIdx]=val;
  dirty=true;
  if (writeThr) {
    file.seek((Node.INTEGER_SIZE + Node.LONG_SIZE) + (Node.INTEGER_SIZE * (topIdx)));
    file.writeInt(val);
    if (topIdx < (Node.FREE_LIST_LIMIT - 1))     file.writeInt(Node.NOT_DEFINED);
  }
}
