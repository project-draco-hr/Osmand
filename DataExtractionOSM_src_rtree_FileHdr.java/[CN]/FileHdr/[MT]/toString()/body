{
  try {
    String str=new String();
    str+="\nTotal Nodes " + totalNodes;
    str+="\nRoot Index " + rootIndex;
    str+="\nFile length " + file.length();
    if (waiters != null) {
      str+="\nWaiters : total " + waiters.size();
      for (int i=0; i < waiters.size(); i++)       str+="\n" + i + " : "+ waiters.get(i).toString();
    }
    return str;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
