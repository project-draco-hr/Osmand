{
  String pr;
  if (segment.parentRoute != null) {
    pr=" pend=" + segment.parentSegmentEnd + " parent="+ segment.parentRoute.road;
  }
 else {
    pr="";
  }
  println(prefix + "" + segment.road+ " dir="+ segment.getDirectionAssigned()+ " ind="+ segment.getSegmentStart()+ " ds="+ ((float)segment.distanceFromStart)+ " es="+ ((float)segment.distanceToEnd)+ pr);
}
