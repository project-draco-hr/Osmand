{
  return getResultCollection() != null && (getResultCollection().getPhrase() == phrase || (getResultCollection().getPhrase().isLastWord(ObjectType.POI_TYPE) && phrase.isLastWord(ObjectType.POI_TYPE) && getResultCollection().getPhrase().getLastSelectedWord().getWord().equals(phrase.getLastSelectedWord().getWord())&& !getResultCollection().getPhrase().isUnknownSearchWordPresent()&& !phrase.isUnknownSearchWordPresent()));
}
