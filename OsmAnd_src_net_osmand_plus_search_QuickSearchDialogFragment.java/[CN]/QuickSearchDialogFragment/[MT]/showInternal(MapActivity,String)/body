{
  Bundle bundle=new Bundle();
  bundle.putString(QUICK_SEARCH_QUERY_KEY,searchQuery);
  QuickSearchDialogFragment fragment=new QuickSearchDialogFragment();
  fragment.setArguments(bundle);
  mapActivity.getSupportFragmentManager().beginTransaction().add(R.id.fragmentContainer,fragment,TAG).addToBackStack(TAG).commitAllowingStateLoss();
}
