{
  for (int i=from; (i < others.length) && (others[i] != null) && (others[i].getRect().getMinX() <= event.getRect().getMaxX()); i++) {
    if (event.getRect().getMinY() < others[i].getRect().getMaxY() && event.getRect().getMaxY() > others[i].getRect().getMinY()) {
      if (evtSide == Join.LEFT)       pairs.add(p.paired(event,others[i]));
 else       pairs.add(p.paired(others[i],event));
    }
  }
}
