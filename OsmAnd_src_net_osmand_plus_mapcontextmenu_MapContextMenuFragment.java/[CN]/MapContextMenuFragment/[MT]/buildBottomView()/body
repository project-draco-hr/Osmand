{
  View bottomView=view.findViewById(R.id.context_menu_bottom_view);
  if (menu.isExtended()) {
    bottomView.setOnTouchListener(new View.OnTouchListener(){
      @Override public boolean onTouch(      View v,      MotionEvent event){
        return true;
      }
    }
);
    menu.build(bottomView);
  }
}
