{
  if (request.type == SearchAmenityRequest.NEW_SEARCH_INIT) {
    publishProgress(object);
  }
 else   if (request.type == SearchAmenityRequest.SEARCH_FURTHER) {
    if (existingObjects != null && !existingObjects.contains(object.getId())) {
      publishProgress(object);
    }
  }
  return true;
}
