{
  this.leftTileX=leftTileX;
  if (rotate < 0) {
    rotate+=360;
  }
 else   if (rotate > 360) {
    rotate-=360;
  }
  this.rotate=rotate;
  this.tileHeight=tileHeight;
  this.tileWidth=tileWidth;
  this.topTileY=topTileY;
  this.zoom=zoom;
  init();
  latLonBounds=calculateLatLonBox(new QuadRect());
}
