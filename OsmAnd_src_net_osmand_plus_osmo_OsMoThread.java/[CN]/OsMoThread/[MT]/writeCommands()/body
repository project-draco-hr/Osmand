{
  if (pendingSendCommand == null) {
    pendingSendCommand=getNewPendingSendCommand();
  }
  while (pendingSendCommand != null) {
    activeChannel.write(pendingSendCommand);
    if (!pendingSendCommand.hasRemaining()) {
      pendingSendCommand=getNewPendingSendCommand();
    }
 else {
      break;
    }
  }
}
