{
  adapter.item(R.string.layer_osm_bugs).selected(settings.SHOW_OSM_BUGS.get() ? 1 : 0).icons(R.drawable.ic_action_bug_dark,R.drawable.ic_action_bug_light).listen(new OnContextMenuClick(){
    @Override public boolean onContextMenuClick(    int itemId,    int pos,    boolean isChecked,    DialogInterface dialog){
      if (itemId == R.string.layer_osm_bugs) {
        settings.SHOW_OSM_BUGS.set(isChecked);
      }
      return true;
    }
  }
).position(7).reg();
}
