{
  if (fragment instanceof AvailableGPXFragment) {
    adapter.item(R.string.local_index_mi_upload_gpx).icons(R.drawable.ic_action_gup_dark,R.drawable.ic_action_gup_light).listen(new OnContextMenuClick(){
      @Override public boolean onContextMenuClick(      int itemId,      int pos,      boolean isChecked){
        sendGPXFiles(la,(AvailableGPXFragment)fragment,(GpxInfo)info);
        return true;
      }
    }
).reg();
  }
}
