{
  if (vrt.isMute()) {
    StringBuilder bld=new StringBuilder();
    for (    String s : builder.execute()) {
      bld.append(s).append(' ');
    }
    sendAlertToAndroidWear(ctx,bld.toString());
    return;
  }
  filesToPlay.addAll(builder.execute());
  if (mediaPlayer == null) {
    requestAudioFocus();
    playQueue();
  }
}
