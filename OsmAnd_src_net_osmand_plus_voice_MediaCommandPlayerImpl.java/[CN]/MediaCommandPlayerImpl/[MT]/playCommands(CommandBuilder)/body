{
  filesToPlay.addAll(builder.execute());
  if (mediaPlayer == null) {
    if (android.os.Build.VERSION.SDK_INT >= 8) {
      mAudioFocusHelper=new AudioFocusHelper(mCtx);
    }
 else {
      mAudioFocusHelper=null;
    }
    if (mAudioFocusHelper != null)     mAudioFocusHelper.requestFocus();
    playQueue();
  }
}
