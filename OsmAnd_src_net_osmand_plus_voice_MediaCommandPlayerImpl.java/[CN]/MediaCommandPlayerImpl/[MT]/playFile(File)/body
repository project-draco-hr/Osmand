{
  log.debug("Playing file : " + file);
  try {
    mediaPlayer.reset();
    mediaPlayer.setAudioStreamType(streamType);
    mediaPlayer.setDataSource(file.getAbsolutePath());
    mediaPlayer.prepare();
    mediaPlayer.setOnCompletionListener(this);
    mediaPlayer.start();
  }
 catch (  Exception e) {
    log.error("Error while playing voice command",e);
    playQueue();
  }
}
