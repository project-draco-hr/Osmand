{
  if (o instanceof DownloadMapObject) {
    DownloadMapObject mapObject=((DownloadMapObject)o);
    return new PointDescription(PointDescription.POINT_TYPE_WORLD_REGION,view.getContext().getString(R.string.shared_string_map),mapObject.worldRegion.getLocaleName());
  }
  return new PointDescription(PointDescription.POINT_TYPE_WORLD_REGION,view.getContext().getString(R.string.shared_string_map),"");
}
