{
  if (!highwaySpeed.containsKey(way.getHighway())) {
    return false;
  }
  int[] s=way.getTypes();
  for (int i=0; i < s.length; i++) {
    RouteTypeRule r=way.region.quickGetEncodingRule(s[i]);
    String k=r.getTag() + "$" + r.getValue();
    if (avoidElements.containsKey(k)) {
      return false;
    }
  }
  return true;
}
