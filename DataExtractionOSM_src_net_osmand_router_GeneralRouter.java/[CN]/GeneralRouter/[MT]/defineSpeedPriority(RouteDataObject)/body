{
  double priority=1;
  for (int i=0; i < road.types.length; i++) {
    RouteTypeRule r=road.region.quickGetEncodingRule(road.types[i]);
    if (highwayPriorities.containsKey(r.getTag() + "$" + r.getValue())) {
      priority*=highwayPriorities.get(r.getTag() + "$" + r.getValue());
    }
  }
  return priority;
}
