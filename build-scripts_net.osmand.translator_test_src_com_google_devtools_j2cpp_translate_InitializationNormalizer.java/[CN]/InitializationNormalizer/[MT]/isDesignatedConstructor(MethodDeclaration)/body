{
  if (!node.isConstructor()) {
    return false;
  }
  Block body=node.getBody();
  if (body == null) {
    return false;
  }
  @SuppressWarnings("unchecked") List<Statement> stmts=body.statements();
  if (stmts.isEmpty()) {
    return true;
  }
  Statement firstStmt=stmts.get(0);
  return !(firstStmt instanceof ConstructorInvocation);
}
