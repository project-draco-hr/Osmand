{
  if (dialect != DBDialect.NOSQL) {
    if (currentCountNode > 0) {
      prepNode.executeBatch();
    }
    prepNode.close();
    if (currentWaysCount > 0) {
      prepWays.executeBatch();
    }
    prepWays.close();
    if (currentRelationsCount > 0) {
      prepRelations.executeBatch();
    }
    prepRelations.close();
  }
 else {
    database.write(options,batch);
  }
}
