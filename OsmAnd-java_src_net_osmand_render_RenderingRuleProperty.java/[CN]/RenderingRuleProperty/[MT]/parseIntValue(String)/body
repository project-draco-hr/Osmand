{
  if (type == INT_TYPE) {
    try {
      int colon=value.indexOf(':');
      if (colon != -1) {
        int c=0;
        if (colon > 0) {
          c+=Integer.parseInt(value.substring(0,colon));
        }
        c+=Integer.parseInt(value.substring(colon + 1));
        return c;
      }
      return Integer.parseInt(value);
    }
 catch (    NumberFormatException e) {
      log.error("Rendering parse " + value);
    }
    return -1;
  }
 else   if (type == BOOLEAN_TYPE) {
    return Boolean.parseBoolean(value) ? TRUE_VALUE : FALSE_VALUE;
  }
 else   if (type == STRING_TYPE) {
    return -1;
  }
 else   if (type == COLOR_TYPE) {
    try {
      return parseColor(value);
    }
 catch (    RuntimeException e) {
      log.error("Rendering parse " + e.getMessage());
    }
    return -1;
  }
 else {
    return -1;
  }
}
