{
  if (type == FLOAT_TYPE) {
    try {
      int colon=value.indexOf(':');
      if (colon != -1) {
        if (colon > 0) {
          return Float.parseFloat(value.substring(0,colon));
        }
        return 0;
      }
      return Float.parseFloat(value);
    }
 catch (    NumberFormatException e) {
      log.error("Rendering parse " + value);
    }
    return -1;
  }
 else {
    return -1;
  }
}
