{
  if (type == FLOAT_TYPE) {
    try {
      int colon=value.indexOf(':');
      if (colon != -1) {
        float c=0;
        if (c > 0) {
          c+=Float.parseFloat(value.substring(0,colon));
        }
        c+=Float.parseFloat(value.substring(colon + 1));
        return c;
      }
      return Float.parseFloat(value);
    }
 catch (    NumberFormatException e) {
      log.error("Rendering parse " + value);
    }
    return -1;
  }
 else {
    return -1;
  }
}
