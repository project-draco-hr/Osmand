{
  try {
    if (mSetExclusiveCheckableMethod == null) {
      mSetExclusiveCheckableMethod=mWrappedObject.getClass().getDeclaredMethod("setExclusiveCheckable",Boolean.TYPE);
    }
    mSetExclusiveCheckableMethod.invoke(mWrappedObject,checkable);
  }
 catch (  Exception e) {
    Log.w(LOG_TAG,"Error while calling setExclusiveCheckable",e);
  }
}
