{
  if (dialect != DBDialect.NOSQL) {
    if (pselectNode != null) {
      pselectNode.close();
    }
    if (pselectWay != null) {
      pselectWay.close();
    }
    if (pselectRelation != null) {
      pselectRelation.close();
    }
    if (pselectTags != null) {
      pselectTags.close();
    }
    if (iterateNodes != null) {
      iterateNodes.close();
    }
    if (iterateRelations != null) {
      iterateRelations.close();
    }
    if (iterateWays != null) {
      iterateWays.close();
    }
    if (iterateWayBoundaries != null) {
      iterateWayBoundaries.close();
    }
  }
}
