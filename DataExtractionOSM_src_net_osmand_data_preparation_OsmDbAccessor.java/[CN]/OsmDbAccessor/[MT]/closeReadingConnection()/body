{
  if (dialect != DBDialect.NOSQL) {
    if (pselectNode != null) {
      pselectNode.close();
    }
    if (pselectWay != null) {
      pselectWay.close();
    }
    if (pselectRelation != null) {
      pselectRelation.close();
    }
    if (pselectTags != null) {
      pselectTags.close();
    }
  }
}
