{
  String l=e.getTag(OSMTagKey.LAYER);
  if (l != null) {
    if (l.startsWith("+")) {
      l=l.substring(1);
    }
    int la=0;
    try {
      la=Integer.parseInt(l);
    }
 catch (    NumberFormatException es) {
    }
    if (la < 0) {
      return 1;
    }
 else     if (la > 0) {
      return 2;
    }
  }
 else   if (e.getTag(OSMTagKey.BRIDGE) != null) {
    return 2;
  }
 else   if (e.getTag(OSMTagKey.TUNNEL) != null) {
    return 1;
  }
  return 0;
}
