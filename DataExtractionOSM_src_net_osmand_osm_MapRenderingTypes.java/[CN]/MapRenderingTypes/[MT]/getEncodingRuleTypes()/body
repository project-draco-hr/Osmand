{
  if (types == null) {
    types=new LinkedHashMap<String,MapRulType>();
    typeList.clear();
    nameRuleType=new MapRulType();
    nameRuleType.tag="name";
    nameRuleType.onlyNameRef=true;
    nameRuleType.additional=false;
    registerRuleType("name",null,nameRuleType);
    init();
  }
  return types;
}
