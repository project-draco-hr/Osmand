{
  if (res.getStartPointIndex() != res.getEndPointIndex()) {
    if (result.size() > 0) {
      RouteSegmentResult last=result.get(result.size() - 1);
      if (last.getObject().id == res.getObject().id) {
        if (combineTwoSegmentResult(res,last,reverse)) {
          return;
        }
      }
    }
    result.add(res);
  }
}
