{
  ArrayList<MyMenuItem> arrayList=new ArrayList<>();
  MyMenuItem.Builder builder=new MyMenuItem.Builder().setTitle(R.string.first_usage_item,getActivity()).setDescription(R.string.first_usage_item_description,getActivity());
  arrayList.add(builder.create());
  builder=new MyMenuItem.Builder().setTitle(R.string.shared_string_navigation,getActivity()).setDescription(R.string.navigation_item_description,getActivity());
  arrayList.add(builder.create());
  builder=new MyMenuItem.Builder().setTitle(R.string.faq_item,getActivity()).setDescription(R.string.faq_item_description,getActivity());
  arrayList.add(builder.create());
  return arrayList;
}
