{
  String[] rules=format.split(";");
  List<OpeningHoursRule> rs=new ArrayList<OpeningHoursRule>();
  for (  String r : rules) {
    r=r.trim();
    if (r.length() == 0) {
      continue;
    }
    OpeningHoursRule rule=parseRule(r);
    if (rule == null) {
      return null;
    }
    rs.add(rule);
  }
  return rs;
}
