{
  int[][] hours=new int[7][2];
  for (int k=0; k < 7; k++) {
    hours[k][0]=hours[k][1]=-1;
  }
  boolean days[]=new boolean[7];
  String[] rules=format.split(";");
  for (  String r : rules) {
    r=r.trim();
    if (r.length() == 0) {
      continue;
    }
    if (!parseRule(r,hours,days)) {
      return null;
    }
  }
  return hours;
}
