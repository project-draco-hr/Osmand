{
  List<OpeningHoursRule> hours=parseOpenedHours("Mo-Fr 08:30-14:40; Sa 08:00 - 14:00");
  System.out.println(hours);
  System.out.println(toStringOpenedHours(hours));
  hours=parseOpenedHours("Mo, We-Fr, Th, Sa 08:30-14:40; Sa 08:00 - 14:00");
  System.out.println(hours);
  System.out.println(toStringOpenedHours(hours));
}
