{
  int[][] hours=parseOpenedHours("Mo-Fr 08:30-14:40; Sa 08:00 - 14:00");
  System.out.println(Arrays.deepToString(hours));
  System.out.println(toStringOpenedHours(hours));
  hours=parseOpenedHours("Mo, We-Fr 08:30-14:40; Sa 08:00 - 14:00");
  System.out.println(Arrays.deepToString(hours));
  System.out.println(toStringOpenedHours(hours));
}
