{
  int l=getNumberOfTips();
  if (suggest >= l || suggest < 0) {
    suggest=0;
  }
  if (l > 0) {
    int it=suggest;
    do {
      if (!shownTips[it]) {
        markTipAsShown(it);
        return it;
      }
      it++;
      if (it == l) {
        it=0;
      }
    }
 while (it != suggest);
    clearShownInfo();
    markTipAsShown(suggest);
    return suggest;
  }
  return -1;
}
