{
  StringBuilder b=new StringBuilder();
  b.append("create table ").append(columns[0].getTableName()).append(" (");
  boolean first=true;
  for (  IndexColumn c : columns) {
    if (first) {
      first=false;
    }
 else {
      b.append(", ");
    }
    b.append(c.toString());
    if (c.getType() != null) {
      b.append(" ").append(c.getType());
    }
 else {
      b.append(" varchar(255)");
    }
  }
  b.append(" )");
  return b.toString();
}
