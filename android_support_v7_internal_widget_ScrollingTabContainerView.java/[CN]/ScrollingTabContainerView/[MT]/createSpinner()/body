{
  final SpinnerCompat spinner=new SpinnerCompat(getContext(),null,R.attr.actionDropDownStyle);
  spinner.setLayoutParams(new LinearLayoutCompat.LayoutParams(LinearLayoutCompat.LayoutParams.WRAP_CONTENT,LinearLayoutCompat.LayoutParams.MATCH_PARENT));
  spinner.setOnItemClickListenerInt(this);
  return spinner;
}
