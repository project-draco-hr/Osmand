{
  if (latitude > LATITUDE_MIN && latitude <= LATITUDE_MAX) {
    return latitude;
  }
  while (latitude < -90 || latitude > 90) {
    if (latitude < 0) {
      latitude+=LATITUDE_TURN;
    }
 else {
      latitude-=LATITUDE_TURN;
    }
  }
  if (latitude < LATITUDE_MIN) {
    return LATITUDE_MIN;
  }
 else   if (latitude > LATITUDE_MAX) {
    return LATITUDE_MAX;
  }
  return latitude;
}
