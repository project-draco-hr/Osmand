{
  while (latitude < -90 || latitude > 90) {
    if (latitude < 0) {
      latitude+=LATITUDE_TURN;
    }
 else {
      latitude-=LATITUDE_TURN;
    }
  }
  if (latitude < MIN_LATITUDE) {
    return MIN_LATITUDE;
  }
 else   if (latitude > MAX_LATITUDE) {
    return MAX_LATITUDE;
  }
  return latitude;
}
