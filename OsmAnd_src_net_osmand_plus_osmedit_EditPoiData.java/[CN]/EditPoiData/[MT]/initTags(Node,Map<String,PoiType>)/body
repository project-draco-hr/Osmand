{
  checkNotInEdit();
  for (  String s : node.getTagKeySet()) {
    tryAddTag(s,node.getTag(s));
  }
  String subType=amenity.getSubType();
  String value="";
  PoiType pt=amenity.getType().getPoiTypeByKeyName(subType);
  if (pt != null) {
    value=pt.getTranslation();
  }
  tagValues.put(POI_TYPE_TAG,value);
}
