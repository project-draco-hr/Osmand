{
  if (name.equals("view")) {
    name=attrs.getAttributeValue(null,"class");
  }
  try {
    mConstructorArgs[0]=context;
    mConstructorArgs[1]=attrs;
    if (-1 == name.indexOf('.')) {
      return createView(name,"android.widget.");
    }
 else {
      return createView(name,null);
    }
  }
 catch (  Exception e) {
    return null;
  }
 finally {
    mConstructorArgs[0]=null;
    mConstructorArgs[1]=null;
  }
}
