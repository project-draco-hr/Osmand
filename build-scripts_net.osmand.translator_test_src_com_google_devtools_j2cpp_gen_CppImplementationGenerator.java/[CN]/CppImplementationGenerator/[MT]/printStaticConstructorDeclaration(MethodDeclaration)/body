{
  String className=NameTable.javaTypeToCpp(Types.getMethodBinding(m).getDeclaringClass(),false);
  StringBuffer sb=new StringBuffer();
  sb.append("{\nif (self == [" + className + " class]) {\n");
  @SuppressWarnings("unchecked") List<Statement> statements=m.getBody().statements();
  for (  Statement statement : statements) {
    sb.append(generateStatement(statement,false,true));
  }
  sb.append("}\n}");
  print("+ (void)initialize " + reindent(sb.toString()) + "\n\n");
}
