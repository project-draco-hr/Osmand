{
  if (!toType.isArray()) {
    return false;
  }
  if (toType.getElementType().isPrimitive()) {
    return primitiveElementType != null && primitiveElementType.isAssignmentCompatible(toType.getElementType());
  }
 else   if (primitiveElementType == null) {
    return true;
  }
 else {
    return elementType.isAssignmentCompatible(toType.getElementType());
  }
}
