{
  BufferedReader reader=new BufferedReader(new FileReader(f));
  try {
    String line;
    if (matchers == null) {
      matchers=new ArrayList<Pattern>();
    }
    while ((line=reader.readLine()) != null) {
      matchers.add(Pattern.compile(line));
    }
  }
  finally {
    Algoritms.closeStream(reader);
  }
}
