{
  if (!f.isFile() && !f.getName().endsWith(IndexBatchCreator.GEN_LOG_EXT)) {
    return false;
  }
  boolean matches=internalPatternMatches(f.getName(),matchers);
  if (matches && exculdeMatchers != null && internalPatternMatches(f.getName(),exculdeMatchers)) {
    return false;
  }
  return matches;
}
