{
switch (format) {
case FORMAT_DEGREES:
    return ctx.getString(R.string.navigate_point_format_D);
case FORMAT_MINUTES:
  return ctx.getString(R.string.navigate_point_format_DM);
case FORMAT_SECONDS:
return ctx.getString(R.string.navigate_point_format_DMS);
case UTM_FORMAT:
return "UTM";
default :
return "Unknown format";
}
}
