{
  if (s != null && s.length() > 0) {
    int in=s.indexOf('#');
    if (in >= 0) {
      String nm=s.substring(in + 1).trim();
      String tp=s.substring(0,in);
      if (tp.contains(".")) {
        return new PointDescription(tp.substring(0,tp.indexOf('.')),tp.substring(tp.indexOf('.') + 1),nm);
      }
 else {
        return new PointDescription(tp,nm);
      }
    }
  }
  return null;
}
