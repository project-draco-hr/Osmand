{
  String[] fnames=dir.list();
  if (fnames != null) {
    String fn=null;
    for (    String f : fnames) {
      if (f.contains("WW15MGH")) {
        fn=f;
        break;
      }
    }
    if (fn != null) {
      this.f=new File(dir,fn);
      if (f.exists()) {
        try {
          rf=new RandomAccessFile(f,"r");
        }
 catch (        FileNotFoundException e) {
          log.error("Error",e);
        }
      }
    }
  }
}
