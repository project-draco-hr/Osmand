{
switch (descriptor.getJavaType()) {
case MESSAGE:
    if (type.isInstance(value)) {
      return value;
    }
 else {
      return messageDefaultInstance.newBuilderForType().mergeFrom((Message)value).build();
    }
case ENUM:
  return invokeOrDie(enumValueOf,null,(EnumValueDescriptor)value);
default :
return value;
}
}
