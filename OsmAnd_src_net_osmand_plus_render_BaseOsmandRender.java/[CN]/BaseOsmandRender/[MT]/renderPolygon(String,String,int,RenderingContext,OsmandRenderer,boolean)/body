{
  boolean r=renderPolygonImpl(tag,val,zoom,rc,o,nightMode);
  if (!r) {
    r=renderPolygonImpl(tag,null,zoom,rc,o,nightMode);
  }
  if (!r) {
    for (    BaseOsmandRender d : dependRenderers) {
      r=d.renderPolygon(tag,val,zoom,rc,o,nightMode);
      if (r) {
        break;
      }
    }
  }
  return r;
}
