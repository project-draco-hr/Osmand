{
  Integer i=getPointIconImpl(tag,val,zoom,nightMode);
  if (i == null) {
    i=getPointIconImpl(tag,null,zoom,nightMode);
  }
  if (i == null) {
    for (    BaseOsmandRender d : dependRenderers) {
      i=d.getPointIcon(tag,val,zoom,nightMode);
      if (i != null) {
        break;
      }
    }
  }
  return i;
}
