{
  if (info.icon != null) {
    buildRow(view,info.icon,info.text,info.textPrefix,info.textColor,info.isWiki,info.isText,info.needLinks,info.isPhoneNumber,info.isUrl);
  }
 else   if (info.iconId != 0) {
    buildRow(view,info.iconId,info.text,info.textPrefix,info.textColor,info.isWiki,info.isText,info.needLinks,info.isPhoneNumber,info.isUrl);
  }
}
