{
  final int[] running=new int[]{-1};
  ArrayAdapter<Object> listAdapter=waypointDialogHelper.getWaypointsDrawerAdapter(mapActivity,running,flat);
  OnItemClickListener listener=waypointDialogHelper.getDrawerItemClickListener(mapActivity,running,listAdapter,null);
