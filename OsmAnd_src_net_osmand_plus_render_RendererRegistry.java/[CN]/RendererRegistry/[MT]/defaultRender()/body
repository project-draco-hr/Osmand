{
  if (defaultRender == null) {
    defaultRender=getRenderer(DEFAULT_RENDER);
    if (defaultRender == null) {
      try {
        defaultRender=new BaseOsmandRender();
        defaultRender.init(OsmandRenderingRulesParser.class.getResourceAsStream("default.render.xml"));
      }
 catch (      IOException e) {
        log.error("Exception initialize renderer",e);
      }
catch (      SAXException e) {
        log.error("Exception initialize renderer",e);
      }
    }
  }
  return defaultRender;
}
