{
  if (mInChangeMarkerPositionMode) {
    return true;
  }
  boolean res=false;
  for (  OsmandMapLayer lt : view.getLayers()) {
    if (lt instanceof ContextMenuLayer.IContextMenuProvider) {
      if (((IContextMenuProvider)lt).disableSingleTap()) {
        res=true;
        break;
      }
    }
  }
  return res;
}
