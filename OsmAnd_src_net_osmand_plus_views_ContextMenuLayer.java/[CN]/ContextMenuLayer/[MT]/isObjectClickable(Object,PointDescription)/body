{
  boolean res;
  if (pointDescription != null) {
    res=pointDescription.isParking() || pointDescription.isFavorite() || pointDescription.isAudioNote()|| pointDescription.isPhotoNote()|| pointDescription.isVideoNote()|| pointDescription.isPoi()|| object instanceof OsmPoint|| object instanceof FavouritePoint|| object instanceof OsMoDevice|| object instanceof OpenStreetNote;
  }
 else {
    res=object instanceof Amenity || object instanceof Recording || object instanceof OsmPoint|| object instanceof FavouritePoint|| object instanceof OsMoDevice|| object instanceof OpenStreetNote;
  }
  return res;
}
