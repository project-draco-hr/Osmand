{
  if (disableLongPressOnMap()) {
    return false;
  }
  if (pressedContextMarker(tileBox,point.x,point.y)) {
    Vibrator vibrator=(Vibrator)activity.getSystemService(Context.VIBRATOR_SERVICE);
    vibrator.vibrate(VIBRATE_SHORT);
    mMarkerLocation=menu.getLatLon();
    MapHelper mapHelper=new MapHelper(activity,132);
    mapHelper.showOnMap(mMarkerLocation,true);
    return true;
  }
  showContextMenu(point,tileBox,true);
  view.refreshMap();
  return true;
}
