{
  ArrayList<OsMoDroidPoint> result=new ArrayList<OsMoDroidPoint>();
  try {
    for (int i=0; i < mIRemoteService.getNumberOfPoints(id); i++) {
      result.add(new OsMoDroidPoint(mIRemoteService.getPointLat(id,mIRemoteService.getPointId(id,i)),mIRemoteService.getPointLon(id,mIRemoteService.getPointId(id,i)),mIRemoteService.getPointName(id,mIRemoteService.getPointId(id,i)),mIRemoteService.getPointDescription(id,mIRemoteService.getPointId(id,i)),mIRemoteService.getPointId(id,i),id,null,mIRemoteService.getPointColor(id,mIRemoteService.getPointId(id,i))));
    }
  }
 catch (  RemoteException e) {
    log.error(e.getMessage(),e);
  }
  return result;
}
