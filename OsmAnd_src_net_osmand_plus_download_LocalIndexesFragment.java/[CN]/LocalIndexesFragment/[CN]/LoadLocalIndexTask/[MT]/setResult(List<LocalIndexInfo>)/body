{
  this.result=result;
  if (result == null) {
    listAdapter.clear();
  }
 else {
    for (    LocalIndexInfo v : result) {
      listAdapter.addLocalIndexInfo(v);
      descriptionLoader=new LoadLocalIndexDescriptionTask();
      descriptionLoader.execute(v);
    }
    listAdapter.notifyDataSetChanged();
    expandAllGroups();
    onPostExecute(result);
  }
}
