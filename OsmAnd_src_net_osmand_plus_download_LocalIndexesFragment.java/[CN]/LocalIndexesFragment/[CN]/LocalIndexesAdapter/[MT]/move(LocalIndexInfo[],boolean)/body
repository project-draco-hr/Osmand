{
  for (  LocalIndexInfo i : values) {
    LocalIndexInfo c=findCategory(i,oldBackupState);
    if (c != null) {
      data.get(c).remove(i);
    }
    c=findCategory(i,!oldBackupState);
    if (c != null) {
      data.get(c).add(i);
    }
  }
  listAdapter.notifyDataSetChanged();
  expandAllGroups();
}
