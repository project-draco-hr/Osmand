{
  List<HistoryEntry> entries=new ArrayList<HistoryEntry>();
  SQLiteDatabase db=getReadableDatabase();
  if (db != null) {
    Cursor query=db.rawQuery("SELECT " + HISTORY_COL_NAME + ", "+ HISTORY_COL_LAT+ ","+ HISTORY_COL_LON+ " FROM "+ HISTORY_TABLE_NAME+ " ORDER BY "+ HISTORY_COL_TIME+ " DESC",null);
    if (query.moveToFirst()) {
      do {
        HistoryEntry e=new HistoryEntry(query.getDouble(1),query.getDouble(2),query.getString(0));
        entries.add(e);
      }
 while (query.moveToNext());
    }
    query.close();
  }
  return entries;
}
