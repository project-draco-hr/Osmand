{
  List<LocationPoint> favs=new ArrayList<LocationPoint>();
  getFavoriteFromPoint(tileBox,point,favs);
  if (favs.size() > 0) {
    SherpafyCustomization customization=(SherpafyCustomization)app.getAppCustomization();
    if (view.getContext() instanceof SherlockFragmentActivity) {
      customization.showFavoriteDialog((SherlockFragmentActivity)view.getContext(),givenStage != null ? givenStage : customization.getSelectedStage(),(StageFavorite)favs.get(0));
      return true;
    }
  }
  return false;
}
