{
  this.app=app;
  this.givenStage=givenStage;
  if (givenStage != null) {
    cachedFavorites=new ArrayList<StageFavorite>();
    for (    Object o : givenStage.getFavorites()) {
      if (o instanceof StageFavorite) {
        StageFavorite sf=(StageFavorite)o;
        cachedFavorites.add(sf);
      }
    }
  }
}
