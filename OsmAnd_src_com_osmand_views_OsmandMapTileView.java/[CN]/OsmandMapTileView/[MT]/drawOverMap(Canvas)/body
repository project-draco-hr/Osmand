{
  int w=getCenterPointX();
  int h=getCenterPointY();
  if (showMapPosition) {
    canvas.drawCircle(w,h,3,paintCenter);
    canvas.drawCircle(w,h,7,paintCenter);
  }
  for (  OsmandMapLayer layer : layers) {
    canvas.restore();
    canvas.save();
    if (!layer.drawInScreenPixels()) {
      canvas.rotate(rotate,w,h);
    }
    layer.onDraw(canvas);
  }
}
