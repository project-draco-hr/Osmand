{
  int w=getCenterPointX();
  int h=getCenterPointY();
  canvas.drawCircle(w,h,3,paintBlack);
  canvas.drawCircle(w,h,6,paintBlack);
  for (  OsmandMapLayer layer : layers) {
    canvas.restore();
    canvas.save();
    if (!layer.drawInScreenPixels()) {
      canvas.rotate(rotate,w,h);
    }
    layer.onDraw(canvas);
  }
}
