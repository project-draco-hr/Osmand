{
  stopAnimatingSync();
  targetZoom=0;
  this.notifyListener=false;
  if (zoomStart < zoomEnd) {
    dirZ=1;
  }
 else {
    dirZ=-1;
  }
  curZ=zoomStart;
  endZ=zoomEnd;
  timeZEnd=600;
  phaseOfMoving=2;
  animateDrag=false;
  time=System.currentTimeMillis();
  stopped=false;
  Thread thread=new Thread(this,"Animatable dragging");
  thread.start();
}
