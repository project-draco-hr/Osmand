{
  createMenuItem(menu,OK_ID,R.string.shared_string_ok,R.drawable.ic_show_on_map,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  createMenuItem(menu,NAVIGATE_DIALOG_ID,R.string.navigate_dialog,R.drawable.ic_action_gdirections_dark,MenuItemCompat.SHOW_AS_ACTION_IF_ROOM);
  return super.onCreateOptionsMenu(menu);
}
