{
  if (plugin.getGpx() != null) {
    this.gpx=plugin.getGpx();
  }
 else {
    GpxUiHelper.selectGPXFile(this,false,false,new CallbackWithObject<GPXUtilities.GPXFile[]>(){
      @Override public boolean processResult(      GPXUtilities.GPXFile[] result){
        gpx=result[0];
        plugin.setGpx(gpx);
        prepareView();
        return false;
      }
    }
);
  }
}
