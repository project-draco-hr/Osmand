{
  name=parser.isNamespaceAware() ? localName : name;
  if ("filter".equals(name)) {
    FilterState st=parseFilterAttributes(attributes);
    stack.push(st);
  }
 else   if ("order".equals(name)) {
    state=ORDER_STATE;
  }
 else   if ("text".equals(name)) {
    state=TEXT_STATE;
  }
 else   if ("point".equals(name)) {
    state=POINT_STATE;
  }
 else   if ("line".equals(name)) {
    state=LINE_STATE;
  }
 else   if ("polygon".equals(name)) {
    state=POLYGON_STATE;
  }
 else   if ("switch".equals(name)) {
    SwitchState st=new SwitchState();
    stack.push(st);
  }
 else   if ("case".equals(name)) {
    FilterState st=parseFilterAttributes(attributes);
    ((SwitchState)stack.peek()).filters.add(st);
  }
 else   if ("renderer".equals(name)) {
    String dc=attributes.getValue("defaultColor");
    int defaultColor=0;
    if (dc != null && dc.length() > 0) {
      defaultColor=parseColor(dc);
    }
    visitor.rendering(attributes.getValue("name"),attributes.getValue("depends"),defaultColor);
  }
 else {
    log.warn("Unknown tag" + name);
  }
}
