{
  super.onStop();
  if (!dismissing) {
    menu.onStop();
  }
  menu.getMapActivity().getContextMenu().setBaseFragmentVisibility(true);
  menu.getMapActivity().getMapLayers().getMapControlsLayer().setControlsClickable(true);
  if (!wasDrawerDisabled) {
    menu.getMapActivity().enableDrawer();
  }
}
