{
  try {
    fileHdr.lockWrite();
    if (slotLvl == Node.NOT_DEFINED)     return;
    long root=fileHdr.getRootIndex();
    Node node=chdNodes.getNode(fileHdr.getFile(),fileName,root,fileHdr);
    cleanUpRec(node,0);
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RTreeException(e.getMessage());
  }
 finally {
    fileHdr.unlock();
  }
}
