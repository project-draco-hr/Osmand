{
  try {
    long sdingRoot=sdingTree.getFileHdr().getRootIndex();
    Node sdingNode=sdingTree.getReadNode(sdingRoot);
    seedRec(sdingNode,chdNodes.getNode(fileHdr.getFile(),fileName,Node.NOT_DEFINED,sdingNode.getElementType(),fileHdr),0);
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RTreeException(e.getMessage());
  }
}
