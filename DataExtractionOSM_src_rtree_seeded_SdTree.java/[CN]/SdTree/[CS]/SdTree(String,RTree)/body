{
  super(fileName);
  try {
    System.out.println("SdTree:: sedding height is " + sdingTree.getHeight());
    fileHdr.lockWrite();
    try {
      fileHdr.setBufferPolicy(false);
      if (sdingTree == null)       throw new IllegalArgumentException("SdTree: Seeding tree is null");
      this.sdingTree=sdingTree;
      setSlot();
      if (slotLvl >= 1)       seed();
    }
 catch (    Exception e) {
      throw new RTreeException(e.getMessage());
    }
  }
  finally {
    fileHdr.unlock();
  }
}
