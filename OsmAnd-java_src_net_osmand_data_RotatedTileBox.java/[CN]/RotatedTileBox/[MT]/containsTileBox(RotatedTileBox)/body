{
  checkTileRectangleCalculated();
  box=box.copy();
  box.checkTileRectangleCalculated();
  if (!containsTilePoint(box.tileLB)) {
    return false;
  }
  if (!containsTilePoint(box.tileLT)) {
    return false;
  }
  if (!containsTilePoint(box.tileRB)) {
    return false;
  }
  if (!containsTilePoint(box.tileRT)) {
    return false;
  }
  return true;
}
