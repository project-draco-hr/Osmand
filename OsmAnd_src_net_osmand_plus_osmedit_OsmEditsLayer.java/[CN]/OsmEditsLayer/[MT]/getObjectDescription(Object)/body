{
  if (o instanceof OsmPoint) {
    OsmPoint point=(OsmPoint)o;
    if (point.getGroup() == OsmPoint.Group.POI) {
      return ((OpenstreetmapPoint)point).getName();
    }
 else     if (point.getGroup() == OsmPoint.Group.BUG) {
      return ((OsmNotesPoint)point).getText();
    }
  }
  return null;
}
