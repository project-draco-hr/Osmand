{
  PoiFilterDbHelper helper=openDbHelper();
  if (helper == null) {
    return false;
  }
  boolean res=helper.addFilter(filter,helper.getWritableDatabase(),false);
  if (res) {
    cacheUserDefinedFilters.add(filter);
    sortListOfFiltersByName(cacheUserDefinedFilters);
  }
  helper.close();
  return res;
}
