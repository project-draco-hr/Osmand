{
  if (customPOIFilter == null) {
    PoiLegacyFilter filter=new PoiLegacyFilter(application.getString(R.string.poi_filter_custom_filter),PoiLegacyFilter.CUSTOM_FILTER_ID,new LinkedHashMap<PoiCategory,LinkedHashSet<String>>(),application);
    filter.setStandardFilter(true);
    customPOIFilter=filter;
  }
  return customPOIFilter;
}
