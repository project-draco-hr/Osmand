{
  if (customPOIFilter == null) {
    PoiUIFilter filter=new PoiUIFilter(application.getString(R.string.poi_filter_custom_filter),PoiUIFilter.CUSTOM_FILTER_ID,new LinkedHashMap<PoiCategory,LinkedHashSet<String>>(),application);
    filter.setStandardFilter(true);
    customPOIFilter=filter;
  }
  return customPOIFilter;
}
