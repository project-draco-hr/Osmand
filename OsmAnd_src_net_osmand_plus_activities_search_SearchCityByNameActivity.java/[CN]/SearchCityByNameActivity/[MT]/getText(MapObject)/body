{
  LatLon l=obj.getLocation();
  if (getFilter().length() > 2 && locationToSearch != null && l != null) {
    return obj.getName(region.useEnglishNames()) + " - " + OsmAndFormatter.getFormattedDistance((int)MapUtils.getDistance(l,locationToSearch),this);
  }
 else {
    return obj.getName(region.useEnglishNames());
  }
}
