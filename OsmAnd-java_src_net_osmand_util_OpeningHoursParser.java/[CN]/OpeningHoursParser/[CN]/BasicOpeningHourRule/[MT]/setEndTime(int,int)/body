{
  if (position > endTimes.length) {
    throw new IllegalArgumentException("It is possible to create only 1 new position." + " Size=" + endTimes.length + ", position="+ position);
  }
 else   if (position == endTimes.length) {
    endTimes=addValueToArray(endTimes,s);
    startTimes=addValueToArray(startTimes,0);
  }
 else {
    endTimes[position]=s;
  }
}
