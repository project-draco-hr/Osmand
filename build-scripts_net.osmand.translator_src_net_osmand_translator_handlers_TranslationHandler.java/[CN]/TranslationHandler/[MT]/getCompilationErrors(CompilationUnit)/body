{
  List<IProblem> errors=Lists.newArrayList();
  for (  IProblem problem : unit.getProblems()) {
    if (problem.isError()) {
      if (((problem.getID() & IProblem.ImportRelated) > 0) && Options.ignoreMissingImports()) {
        continue;
      }
 else {
        errors.add(problem);
      }
    }
  }
  return errors;
}
