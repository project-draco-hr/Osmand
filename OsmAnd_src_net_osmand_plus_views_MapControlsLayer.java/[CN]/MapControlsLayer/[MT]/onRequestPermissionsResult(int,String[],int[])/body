{
  if (requestCode == REQUEST_LOCATION_FOR_NAVIGATION_PERMISSION && grantResults[0] == PackageManager.PERMISSION_GRANTED) {
    onNavigationClick();
  }
 else   if (requestCode == REQUEST_LOCATION_FOR_NAVIGATION_FAB_PERMISSION && grantResults[0] == PackageManager.PERMISSION_GRANTED) {
    navigateFab();
  }
}
