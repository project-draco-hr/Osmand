{
  preloadWayNodes(street2);
  preloadWayNodes(street);
  for (  Way w : street2.getWayNodes()) {
    for (    Way w2 : street.getWayNodes()) {
      for (      Node n : w.getNodes()) {
        for (        Node n2 : w2.getNodes()) {
          if (n.getId() == n2.getId()) {
            return n.getLatLon();
          }
        }
      }
    }
  }
  return null;
}
