{
switch (getType()) {
case SHOP:
    return OSMTagKey.SHOP.getValue();
case LEISURE:
  return OSMTagKey.LEISURE.getValue();
case HISTORIC:
return OSMTagKey.HISTORIC.getValue();
case TOURISM:
return OSMTagKey.TOURISM.getValue();
case SPORT:
return OSMTagKey.SPORT.getValue();
default :
return OSMTagKey.AMENITY.getValue();
}
}
