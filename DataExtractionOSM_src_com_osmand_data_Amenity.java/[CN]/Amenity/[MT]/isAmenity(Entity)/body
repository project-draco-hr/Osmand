{
  if (!(n instanceof Node)) {
    return false;
  }
  if (n.getTag(OSMTagKey.AMENITY) != null) {
    return true;
  }
 else   if (n.getTag(OSMTagKey.SHOP) != null) {
    return true;
  }
 else   if (n.getTag(OSMTagKey.LEISURE) != null) {
    return true;
  }
 else   if (n.getTag(OSMTagKey.TOURISM) != null) {
    return true;
  }
  return false;
}
