{
  WptPt pt=new WptPt();
  pt.lat=current.getLatitude();
  pt.lon=current.getLongitude();
  if (current.hasAltitude()) {
    pt.ele=current.getAltitude();
  }
  if (current.hasSpeed()) {
    pt.speed=current.getSpeed();
  }
  if (current.hasAccuracy()) {
    pt.hdop=current.getAccuracy();
  }
  pt.time=current.getTime();
  pt.name=name;
  return pt;
}
