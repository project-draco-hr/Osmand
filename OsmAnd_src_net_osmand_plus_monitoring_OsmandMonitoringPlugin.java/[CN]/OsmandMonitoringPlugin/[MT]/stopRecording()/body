{
  settings.SAVE_GLOBAL_TRACK_TO_GPX.set(false);
  if (app.getNavigationService() != null) {
    NotificationManager mNotificationManager=(NotificationManager)app.getNavigationService().getSystemService(Context.NOTIFICATION_SERVICE);
    mNotificationManager.cancel(notificationId);
    app.getNavigationService().stopIfNeeded(app,NavigationService.USED_BY_GPX);
  }
}
