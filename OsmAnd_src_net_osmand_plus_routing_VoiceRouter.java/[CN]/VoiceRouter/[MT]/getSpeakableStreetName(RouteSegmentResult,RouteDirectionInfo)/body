{
  if (i == null || !router.getSettings().SPEAK_STREET_NAMES.get()) {
    return empty;
  }
  if (player != null && player.supportsStructuredStreetNames()) {
    Struct next=new Struct(new Term[]{getTermString(getSpeakablePointName(i.getRef())),getTermString(getSpeakablePointName(i.getStreetName())),getTermString(getSpeakablePointName(i.getDestinationName()))});
    Term current=empty;
    if (currentSegment != null) {
      RouteDataObject obj=currentSegment.getObject();
      current=new Struct(new Term[]{getTermString(getSpeakablePointName(obj.getRef())),getTermString(getSpeakablePointName(obj.getName())),getTermString(getSpeakablePointName(obj.getDestinationName()))});
    }
    Struct voice=new Struct("voice",next,current);
    return voice;
  }
 else {
    Term rf=getTermString(getSpeakablePointName(i.getRef()));
    if (rf == empty) {
      rf=getTermString(getSpeakablePointName(i.getStreetName()));
    }
    return rf;
  }
}
