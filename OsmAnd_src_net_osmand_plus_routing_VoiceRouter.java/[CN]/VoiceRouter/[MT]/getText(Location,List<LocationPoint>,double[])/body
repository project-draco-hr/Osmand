{
  String text="";
  for (  LocationPoint point : points) {
    if (text.length() == 0) {
      if (location != null && dist != null) {
        dist[0]=MapUtils.getDistance(location.getLatitude(),location.getLongitude(),point.getLatitude(),point.getLongitude());
      }
    }
 else {
      text+=", ";
    }
    text+=point.getName(router.getApplication());
  }
  return text;
}
