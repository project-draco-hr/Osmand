{
  if (pn != null) {
    pn=pn.replace('-',' ');
    pn=pn.replace(':',' ');
    if ((player != null) && (!"de".equals(player.getLanguage()))) {
      pn=pn.replace("\u00df","ss");
    }
    if ((player != null) && ("en".startsWith(player.getLanguage()))) {
      pn=pn.replace("SR","S R");
    }
  }
  return pn;
}
