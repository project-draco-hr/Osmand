{
  DownloadResourceGroup res=null;
  for (  DownloadResourceGroup group : getGroups()) {
    if (group.getIndividualResources() != null) {
      for (      IndexItem i : group.getIndividualResources()) {
        if (i == item) {
          res=group;
          break;
        }
      }
    }
 else {
      res=group.getIndexItemGroup(item);
      if (res != null) {
        break;
      }
    }
  }
  return res;
}
