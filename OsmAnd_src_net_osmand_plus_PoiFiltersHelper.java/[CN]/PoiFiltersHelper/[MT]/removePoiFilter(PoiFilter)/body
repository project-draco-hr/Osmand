{
  if (filter.getFilterId().equals(PoiFilter.CUSTOM_FILTER_ID)) {
    return false;
  }
  PoiFilterDbHelper helper=openDbHelper();
  if (helper == null) {
    return false;
  }
  boolean res=helper.deleteFilter(filter);
  if (res) {
    cacheUserDefinedFilters.remove(filter);
  }
  helper.close();
  return res;
}
