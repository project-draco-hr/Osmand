{
  if (filter.getFilterId().equals(PoiFilter.CUSTOM_FILTER_ID) || filter.getFilterId().equals(PoiFilter.BY_NAME_FILTER_ID)) {
    return false;
  }
  PoiFilterDbHelper helper=openDbHelper();
  if (helper != null) {
    boolean res=helper.editFilter(filter);
    helper.close();
    return res;
  }
  return false;
}
