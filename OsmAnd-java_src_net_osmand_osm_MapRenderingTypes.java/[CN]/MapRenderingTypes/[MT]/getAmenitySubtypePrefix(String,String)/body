{
  Map<String,AmenityRuleType> rules=getAmenityEncodingRuleTypes();
  AmenityRuleType rt=rules.get(constructRuleKey(tag,val));
  if (rt != null && rt.poiPrefix != null) {
    return rt.poiPrefix;
  }
  rt=rules.get(constructRuleKey(tag,null));
  if (rt != null && rt.poiPrefix != null) {
    return rt.poiPrefix;
  }
  return null;
}
