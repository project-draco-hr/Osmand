{
  if (parseDouble(latPosition.getText()) == Double.NaN) {
    JOptionPane.showMessageDialog(this,"Lat coordinate is not a number","Wrong position",JOptionPane.ERROR_MESSAGE);
    return false;
  }
  if (parseDouble(lonPosition.getText()) == Double.NaN) {
    JOptionPane.showMessageDialog(this,"Lon coordinate is not a number","Wrong position",JOptionPane.ERROR_MESSAGE);
    return false;
  }
  result=new LatLon(parseDouble(latPosition.getText()),parseDouble(lonPosition.getText()));
  return true;
}
