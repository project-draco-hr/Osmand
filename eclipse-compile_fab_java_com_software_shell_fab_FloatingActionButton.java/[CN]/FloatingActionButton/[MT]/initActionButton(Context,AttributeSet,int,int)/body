{
  TypedArray attributes=context.getTheme().obtainStyledAttributes(attrs,R.styleable.ActionButton,defStyleAttr,defStyleRes);
  try {
    initType(attributes);
    initShowAnimation(attributes);
    initHideAnimation(attributes);
  }
 catch (  Exception e) {
    Log.e(LOG_TAG,"Unable to read attr",e);
  }
 finally {
    attributes.recycle();
  }
  Log.v(LOG_TAG,"Floating Action Button initialized");
}
