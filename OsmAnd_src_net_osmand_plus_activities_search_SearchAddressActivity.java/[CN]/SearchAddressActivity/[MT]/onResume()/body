{
  super.onResume();
  searchPoint=osmandSettings.getLastSearchedPoint();
  showOnMap.setEnabled(searchPoint != null);
  navigateTo.setEnabled(searchPoint != null);
  region=null;
  postcode=null;
  city=null;
  street=null;
  building=null;
  region=osmandSettings.getLastSearchedRegion();
  loadData();
  updateUI();
}
