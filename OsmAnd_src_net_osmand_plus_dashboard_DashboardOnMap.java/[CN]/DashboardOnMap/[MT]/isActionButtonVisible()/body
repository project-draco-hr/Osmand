{
  return visibleType == DashboardType.DASHBOARD || visibleType == DashboardType.WAYPOINTS || visibleType == DashboardType.WAYPOINTS_FLAT || visibleType == DashboardType.LIST_MENU || visibleType == DashboardType.ROUTE_PREFERENCES || visibleType == DashboardType.CONFIGURE_SCREEN || (visibleType == DashboardType.MAP_MARKERS && mapMarkerDialogHelper.hasActiveMarkers()) || (visibleType == DashboardType.MAP_MARKERS_SELECTION && mapMarkerDialogHelper.hasActiveMarkers());
}
