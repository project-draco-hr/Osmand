import re, sys, string
try:
    unicode('')
except NameError:

    def encode(s, encoding):
        return s
    _escape = re.compile('[&<>\\"\\x80-\\xff]+')
else:

    def encode(s, encoding):
        return s.encode(encoding)
    _escape = re.compile(eval('u"[&<>\\"\\u0080-\\uffff]+"'))
del _escape
