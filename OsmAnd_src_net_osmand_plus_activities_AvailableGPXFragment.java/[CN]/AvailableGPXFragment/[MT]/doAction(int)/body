{
  if (actionResId == R.string.local_index_mi_delete) {
    operationTask=new DeleteGpxTask();
  }
 else {
    operationTask=null;
  }
  if (operationTask != null) {
    operationTask.execute(selectedItems.toArray(new GpxInfo[selectedItems.size()]));
  }
  if (actionMode != null) {
    actionMode.finish();
  }
}
