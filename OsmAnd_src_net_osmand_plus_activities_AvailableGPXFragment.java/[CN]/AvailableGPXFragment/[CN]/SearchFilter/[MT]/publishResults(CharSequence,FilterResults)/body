{
  if (results.values != null) {
synchronized (listAdapter) {
      listAdapter.clear();
      for (      GpxInfo i : ((List<GpxInfo>)results.values)) {
        listAdapter.addLocalIndexInfo(i);
      }
    }
    listAdapter.notifyDataSetChanged();
    if (constraint != null && constraint.length() > 3) {
      collapseTrees(10);
    }
  }
}
