{
  if (results.values != null) {
synchronized (allGpxAdapter) {
      allGpxAdapter.clear();
      for (      GpxInfo i : ((List<GpxInfo>)results.values)) {
        allGpxAdapter.addLocalIndexInfo(i);
      }
    }
    allGpxAdapter.notifyDataSetChanged();
    if (constraint != null && constraint.length() > 3) {
      collapseTrees(10);
    }
  }
}
