{
  if (gpx == null) {
    gpx=GPXUtilities.loadGPXFile(app,file);
  }
  if (gpx.warning != null) {
    corrupted=true;
    description=gpx.warning;
  }
 else {
    description=GpxUiHelper.getDescription(app,gpx,file) + app.getString(R.string.local_index_gpx_info_show);
  }
  htmlDescription=null;
  getHtmlDescription();
}
