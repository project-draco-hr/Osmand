{
  if (gpx == null) {
    gpx=GPXUtilities.loadGPXFile(app,file);
  }
  if (gpx.warning != null) {
    corrupted=true;
    description=gpx.warning;
    analysis=null;
  }
 else {
    analysis=gpx.getAnalysis(file.lastModified());
    description=GpxUiHelper.getDescription(app,analysis,true);
  }
  htmlDescription=null;
  getHtmlDescription();
}
