{
  int found=-1;
  for (int i=category.size() - 1; i >= 0; i--) {
    String cat=category.get(i);
    if (Algorithms.objectEquals(getActivity().getString(R.string.local_indexes_cat_gpx) + " " + g.subfolder,cat)) {
      found=i;
      break;
    }
  }
  if (found != -1) {
    data.get(category.get(found)).remove(g);
  }
}
