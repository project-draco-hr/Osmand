{
  if (visible.size() == 3) {
    for (    ApplicationMode mode : selected) {
      if (visible.contains(mode)) {
        return;
      }
    }
  }
  visible.clear();
  visible.add(ApplicationMode.DEFAULT);
  for (  ApplicationMode mode : selected) {
    if (mode != ApplicationMode.DEFAULT) {
      visible.add(mode);
      break;
    }
  }
  for (  ApplicationMode mode : values) {
    if (!visible.contains(mode)) {
      visible.add(mode);
      if (visible.size() == 3) {
        break;
      }
    }
  }
}
