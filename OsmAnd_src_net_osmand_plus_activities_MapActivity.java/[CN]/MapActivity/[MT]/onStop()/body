{
  if (routingHelper.isFollowingMode()) {
    mNotificationManager=(NotificationManager)getSystemService(NOTIFICATION_SERVICE);
    mNotificationManager.notify(APP_NOTIFICATION_ID,getNotification());
  }
  if (progressDlg != null) {
    progressDlg.dismiss();
    progressDlg=null;
  }
  super.onStop();
}
