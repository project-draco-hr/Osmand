{
  if (rectangle == null)   throw new IllegalValueException("rtree.Rect.getResultingMBR : Rect is null");
  if (rectangle.isNull())   if (this.isNull())   return new Rect();
 else   return new Rect(this);
 else   if (this.isNull())   return rectangle;
  int topX, topY, botX, botY;
  topX=Math.min(rectangle.getMinX(),minX);
  topY=Math.min(rectangle.getMinY(),minY);
  botX=Math.max(rectangle.getMaxX(),maxX);
  botY=Math.max(rectangle.getMaxY(),maxY);
  return (new Rect(topX,topY,botX,botY));
}
