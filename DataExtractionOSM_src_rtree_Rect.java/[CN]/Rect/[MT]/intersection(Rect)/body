{
  if (rect == null)   throw new IllegalArgumentException("Rect.instersection : Argument Rect is null");
  int x1;
  int y1;
  int x2;
  int y2;
  if (minX < rect.minX)   x1=rect.minX;
 else   x1=minX;
  if (minY < rect.minY)   y1=rect.minY;
 else   y1=minY;
  if (maxX > rect.maxX)   x2=rect.maxX;
 else   x2=maxX;
  if (maxY > rect.maxY)   y2=rect.maxY;
 else   y2=maxY;
  try {
    if (x1 > x2 || y1 > y2)     return new Rect();
 else     return new Rect(x1,y1,x2,y2);
  }
 catch (  Exception e) {
    e.printStackTrace();
    return new Rect();
  }
}
