{
  if (rect == null)   throw new IllegalValueException("Rect.overlaps: null argument");
  int rectMinX=rect.getMinX();
  int rectMinY=rect.getMinY();
  int rectMaxX=rect.getMaxX();
  int rectMaxY=rect.getMaxY();
  boolean ret=false;
  if ((minX == rectMinX) && (minY == rectMinY) && (maxX == rectMaxX)&& (maxY == rectMaxY))   return true;
  if ((minX < rectMaxX) && (maxX > rectMinX))   ret=true;
 else   return false;
  if ((minY < rectMaxY) && (maxY > rectMinY))   ret=true;
 else   return false;
  return ret;
}
