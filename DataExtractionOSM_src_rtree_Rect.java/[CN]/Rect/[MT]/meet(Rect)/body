{
  if (rect == null)   throw new IllegalValueException("rtree.Rect.meet: null argument");
  boolean ret=true;
  if (disjoint(rect))   return false;
  if ((minX < rect.getMaxX()) && (maxX > rect.getMinX()))   if ((minY < rect.getMaxY()) && (maxY > rect.getMinY()))   return false;
  System.out.println("Raj!");
  if ((minX == rect.getMaxX()) || (maxX == rect.getMinX()))   return true;
 else   ret=false;
  if ((minY == rect.getMaxY()) || (maxY == rect.getMinY()))   ret=true;
  return ret;
}
