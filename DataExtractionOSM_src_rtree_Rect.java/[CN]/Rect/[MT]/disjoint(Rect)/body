{
  if (rect == null)   throw new IllegalValueException("rtree.Rect.disjoint: null argument");
  boolean ret=true;
  if ((minX <= rect.getMaxX()) && (maxX >= rect.getMinX()))   ret=false;
 else   return ret;
  if ((minY <= rect.getMaxY()) && (maxY >= rect.getMinY()))   ret=false;
 else   ret=true;
  return ret;
}
