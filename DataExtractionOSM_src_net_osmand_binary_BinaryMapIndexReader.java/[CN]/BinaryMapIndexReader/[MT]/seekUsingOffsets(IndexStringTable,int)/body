{
  initStringOffsets(st,index);
  int shift=0;
  int a=index / IndexStringTable.SIZE_OFFSET_ARRAY;
  if (a > st.offsets.size()) {
    a=st.offsets.size();
  }
  if (a > 0) {
    shift=st.offsets.get(a - 1);
  }
  codedIS.seek(st.fileOffset + shift);
  return index - a * IndexStringTable.SIZE_OFFSET_ARRAY;
}
