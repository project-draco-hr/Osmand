{
  AddressRegion r=getRegionByName(region);
  if (r.postcodesOffset != -1) {
    codedIS.seek(r.postcodesOffset);
    int len=readInt();
    int old=codedIS.pushLimit(len);
    PostCode p=addressAdapter.findPostcode(name);
    if (p != null) {
      return p;
    }
    codedIS.popLimit(old);
  }
  return null;
}
