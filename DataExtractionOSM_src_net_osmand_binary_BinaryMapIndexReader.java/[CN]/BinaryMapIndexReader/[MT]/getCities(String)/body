{
  List<City> cities=new ArrayList<City>();
  AddressRegion r=getRegionByName(region);
  if (r.citiesOffset != -1) {
    codedIS.seek(r.citiesOffset);
    int len=readInt();
    int old=codedIS.pushLimit(len);
    addressAdapter.readCities(cities,null,false);
    codedIS.popLimit(old);
  }
  return cities;
}
