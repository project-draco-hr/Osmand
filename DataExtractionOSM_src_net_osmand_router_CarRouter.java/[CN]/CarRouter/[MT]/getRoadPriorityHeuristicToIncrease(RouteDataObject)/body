{
  String highway=getHighway(road);
  double priority=highway != null && autoPriorityValues.containsKey(highway) ? autoPriorityValues.get(highway) : 0.5d;
  if (priority >= 1) {
    return 1;
  }
 else   if (priority >= 0.7) {
    return 0.7;
  }
 else   if (priority >= 0.5) {
    return 0.5;
  }
 else {
    return 0.3;
  }
}
