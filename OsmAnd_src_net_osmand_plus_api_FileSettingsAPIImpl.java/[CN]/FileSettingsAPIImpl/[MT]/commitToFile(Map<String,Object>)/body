{
  for (  Entry<String,Object> e : modified.entrySet()) {
    if (e.getValue() == null) {
      map.remove(e.getKey());
    }
 else {
      map.put(e.getKey(),e.getValue());
    }
  }
  try {
    Properties ps=new Properties();
    ps.putAll(map);
    final FileOutputStream fout=new FileOutputStream(file);
    ps.store(fout,null);
    fout.close();
    return true;
  }
 catch (  IOException e1) {
    e1.printStackTrace();
    return false;
  }
}
