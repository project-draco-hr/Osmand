{
  SQLiteAPI.SQLiteConnection db=openConnection(false);
  if (db != null) {
    try {
switch (type) {
case DOWNLOAD_ENTRY:
        db.execSQL("INSERT INTO " + DOWNLOADS_TABLE_NAME + " VALUES (?, ?)",new Object[]{e.getName(),e.getCount()});
case FAVORITES_ENTRY:
      db.execSQL("INSERT INTO " + FAVORITES_TABLE_NAME + " VALUES (?, ?)",new Object[]{e.getName(),e.getCount()});
  }
}
  finally {
  db.close();
}
return true;
}
return false;
}
