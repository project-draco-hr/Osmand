{
  super.onListItemClick(l,v,position,id);
  MapObject item=((MapObjectAdapter)getListAdapter()).getItem(position);
  OsmandSettings.getOsmandSettings(this).setMapLocationToShow(item.getLocation().getLatitude(),item.getLocation().getLongitude(),getString(R.string.address) + " : " + item.toString());
  startActivity(new Intent(this,MapActivity.class));
}
