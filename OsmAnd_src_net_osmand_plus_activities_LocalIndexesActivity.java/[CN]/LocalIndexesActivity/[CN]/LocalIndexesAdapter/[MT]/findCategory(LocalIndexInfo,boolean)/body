{
  for (  LocalIndexInfo i : category) {
    if (i.isBackupedData() == backuped && val.getType() == i.getType()) {
      return i;
    }
  }
  LocalIndexInfo newCat=new LocalIndexInfo(val.getType(),backuped);
  category.add(newCat);
  data.put(newCat,new ArrayList<LocalIndexInfo>());
  return newCat;
}
