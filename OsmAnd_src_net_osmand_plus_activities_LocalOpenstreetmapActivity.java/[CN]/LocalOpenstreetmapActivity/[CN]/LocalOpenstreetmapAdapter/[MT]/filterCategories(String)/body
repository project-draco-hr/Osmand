{
  List<String> filter=new ArrayList<String>();
  List<String> source=filterCategory == null ? category : filterCategory;
  for (  String info : source) {
    for (    String ts : types) {
      if (info.compareTo(ts) == 0) {
        filter.add(info);
      }
    }
  }
  filterCategory=filter;
  notifyDataSetChanged();
}
