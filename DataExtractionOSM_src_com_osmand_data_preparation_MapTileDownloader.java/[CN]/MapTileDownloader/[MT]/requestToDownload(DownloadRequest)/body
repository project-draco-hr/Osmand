{
  if (DefaultLauncherConstants.TILE_DOWNLOAD_MAX_ERRORS > 0 && currentErrors > DefaultLauncherConstants.TILE_DOWNLOAD_MAX_ERRORS) {
    return;
  }
  if (request.url == null) {
    return;
  }
  if (!isFileCurrentlyDownloaded(request.fileToSave)) {
    threadPoolExecutor.execute(new DownloadMapWorker(request));
  }
}
