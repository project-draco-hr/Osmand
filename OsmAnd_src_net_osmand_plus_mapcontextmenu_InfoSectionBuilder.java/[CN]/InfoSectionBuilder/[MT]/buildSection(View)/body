{
  MapPoiTypes poiTypes=app.getPoiTypes();
  for (  Map.Entry<String,String> e : amenity.getAdditionalInfo().entrySet()) {
    int iconId=0;
    String key=e.getKey();
    String vl=e.getValue();
    if (key.startsWith("name:")) {
      continue;
    }
 else     if (Amenity.OPENING_HOURS.equals(key)) {
      iconId=R.drawable.mm_clock;
    }
 else     if (Amenity.PHONE.equals(key)) {
      iconId=R.drawable.mm_amenity_telephone;
    }
 else     if (Amenity.WEBSITE.equals(key)) {
      iconId=R.drawable.mm_internet_access;
    }
 else {
      iconId=R.drawable.ic_type_info;
      AbstractPoiType pt=poiTypes.getAnyPoiAdditionalTypeByKey(e.getKey());
      if (pt != null) {
        if (pt instanceof PoiType && !((PoiType)pt).isText()) {
          vl=pt.getTranslation();
        }
 else {
          vl=amenity.unzipContent(e.getValue());
        }
      }
 else {
        vl=amenity.unzipContent(e.getValue());
      }
    }
    buildRow(view,iconId,vl);
  }
}
