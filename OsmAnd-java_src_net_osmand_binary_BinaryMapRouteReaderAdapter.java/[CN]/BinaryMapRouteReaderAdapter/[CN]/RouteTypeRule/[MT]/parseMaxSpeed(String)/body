{
  float floatValue=-1;
  if (v.equals("none")) {
    floatValue=RouteDataObject.NONE_MAX_SPEED;
  }
 else {
    int i=0;
    while (i < v.length() && Character.isDigit(v.charAt(i))) {
      i++;
    }
    if (i > 0) {
      floatValue=Integer.parseInt(v.substring(0,i));
      floatValue/=3.6;
      if (v.contains("mph")) {
        floatValue*=1.6;
      }
    }
  }
  return floatValue;
}
