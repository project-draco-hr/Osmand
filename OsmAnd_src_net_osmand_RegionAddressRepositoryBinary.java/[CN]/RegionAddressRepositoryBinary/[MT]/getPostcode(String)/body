{
  if (name == null) {
    return null;
  }
  String uc=name.toUpperCase();
  if (!postCodes.containsKey(uc)) {
    try {
      postCodes.put(uc,file.getPostcodeByName(this.region,name));
    }
 catch (    IOException e) {
      log.error("Disk operation failed",e);
    }
  }
  return postCodes.get(uc);
}
